EVENT PLAYER
TABLEFALL
IF KEY RIGHT
    TRAIL
    IF X > 228
        SCREENRIGHT
        SCORE 50
		  LET LINE = 22
		  LET COLUMN = 24
		  SHOWSCORE
        LET X = 8
        EXIT
    ELSE
        IF CANGORIGHT
            LET IMAGE = 1
            ANIMATE
            SPRITERIGHT
        ENDIF
    ENDIF
ENDIF
IF KEY LEFT
    TRAIL
    IF X <= 8
        SCREENLEFT
        LET X = 230
        EXIT
    ELSE
        IF CANGOLEFT
            LET IMAGE = 0
            ANIMATE
            SPRITELEFT
        ENDIF
    ENDIF
ENDIF
IF KEY UP
    TABLEJUMP
ENDIF
IF DEADLY
    KILL
ENDIF
DETECTOBJ
IF OBJ = 255
ELSE
    IF OBJ = 0
        GET OBJ
        SCORE 100
		  LET LINE = 22
		  LET COLUMN = 24
		  SHOWSCORE
        LET LINE = 4
        LET COLUMN = 0
        ; COLOUR 2
        MESSAGE 2
        DELAY 40
        WAITKEY
        REDRAW
    ELSE
        IF OBJ = 4
            GET OBJ
            SCORE 100
			  LET LINE = 22
			  LET COLUMN = 24
			  SHOWSCORE
            LET LINE = 4
            LET COLUMN = 0
            ; COLOUR 2
            MESSAGE 3
            DELAY 40
            WAITKEY
            REDRAW
        ELSE
            IF OBJ = 7
                GET OBJ
                SCORE 100
				  LET LINE = 22
				  LET COLUMN = 24
				  SHOWSCORE
                LET LINE = 4
                LET COLUMN = 0
                ; COLOUR 2
                MESSAGE 6
                DELAY 40
                WAITKEY
                REDRAW
            ELSE
                IF OBJ = 8
                    GET OBJ
                    SCORE 150
                    SCORE 150
                    SCORE 150
						  LET LINE = 22
						  LET COLUMN = 24
						  SHOWSCORE
                    LET LINE = 4
                    LET COLUMN = 0
                    ; COLOUR 2
                    MESSAGE 7
                    DELAY 40
                    ENDGAME
                    WAITKEY
                    REDRAW
                ELSE
                    GET OBJ
                    SCORE 100
						  LET LINE = 22
						  LET COLUMN = 24
						  SHOWSCORE
                    BEEP 10
                ENDIF
                IF KEY UP
                    IF LADDERABOVE
                        SPRITEUP
                        LET IMAGE = 6
                        IF A = 0
                            ANIMATE
                        ENDIF
                    ENDIF
                ENDIF
                IF KEY DOWN
                    IF LADDERBELOW
                        SPRITEDOWN
                        LET IMAGE = 6
                        IF A = 0
                            ANIMATE
                        ENDIF
                    ENDIF
                ENDIF
            ENDIF 
        ENDIF
    ENDIF
ENDIF


