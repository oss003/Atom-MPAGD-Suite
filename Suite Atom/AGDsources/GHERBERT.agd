EVENT SPRITETYPE5
IF DIRECTION = 0			; INITIALISE
	LET SETTINGB = Y		; store initial y value in SETTINGB
	LET DIRECTION = 1		; prevent this running again
	LET FRAME = 0
	LET SETTINGA = X
	IF SETTINGA > 100
		SUBTRACT 100 FROM SETTINGA
	ENDIF
ENDIF

IF DIRECTION = 1           
	ANIMATE VERYSLOW
	IF FRAME >= 3
		LET FRAME = 3
	ENDIF

	ADD 2 TO SETTINGA        ; we use SA as the timer
	IF SETTINGA >= 150         ; after 2/5ths of a second
		LET DIRECTION = 2
	ENDIF
ENDIF

IF DIRECTION = 2				; DROP!
	IF CANGODOWN				; if we haven't hit the ground 
		FALL					; drop down
		IF COLLISION PLAYER         ; if the player touches the acorn
			KILL
		ENDIF
	ELSE 						; if we've hit the ground
		LET SETTINGA = 0
		IF FRAME = 3
			CRASH 200
		ENDIF
		ADD 1 TO FRAME			; increment the frame
		IF FRAME >= 7
			LET DIRECTION = 3		; start the HIT THE GROUND routine
		ENDIF
	ENDIF
ENDIF
   
IF DIRECTION = 3				; HIT THE GROUND
	ADD 1 TO SETTINGA			; increment the timer
	IF SETTINGA = 35				; every 1/5th of a second
		LET SETTINGA = 0		; reset the timer
		LET Y = SETTINGB		; reset
		LET DIRECTION = 0		; start the INITALISE routine
		LET FRAME = 0
	ENDIF
ENDIF


