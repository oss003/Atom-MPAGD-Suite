EVENT PLAYER
FALL
LET A Y
LET B X
GET 0
; ASM 205
; ASM 216
; ASM 123
PUT X Y 0
LET Y A
LET X B
IF X > 50
    IF 144 > X
        DIVIDE O BY 8
        MULTIPLY O BY 8
        LET P X
        SUBTRACT O FROM P
        DIVIDE P BY 2
        IF X > 120
            ADD 4 TO P
        ENDIF
        PUT X Y P
    ENDIF
ENDIF
IF KEY RIGHT
    IF CANGORIGHT
        SPRITERIGHT
        IF C = 0
            IF FRAME = 2
                LET FRAME 3
            ELSE
                LET FRAME 2
            ENDIF
        ENDIF
    ENDIF
ENDIF
IF KEY LEFT
    IF CANGOLEFT
        SPRITELEFT
        IF C = 0
            IF FRAME = 0
                LET FRAME 1
            ELSE
                LET FRAME 0
            ENDIF
        ENDIF
    ENDIF
ENDIF
IF KEY DOWN
    SPRITEDOWN
ENDIF
IF KEY UP
