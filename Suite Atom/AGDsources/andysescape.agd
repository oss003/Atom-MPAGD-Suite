EVENT PLAYER
TABLEFALL
LET E = Y
IF F <> 0
    SUBTRACT 1 FROM F
    IF T = 1
        ANIMATE
        GETRANDOM 70
        BEEP RND
    ENDIF
    IF F = 0
        LET IMAGE = SETTINGB
    ENDIF
    EXIT
ENDIF
IF KEY RIGHT
    IF X > 240
        SCREENRIGHT
        LET X = 8
        EXIT
    ELSE
        IF CANGORIGHT
            LET IMAGE = 1
            IF T = 1
                ANIMATE
            ENDIF
            SPRITERIGHT
        ENDIF
    ENDIF
ENDIF
IF KEY LEFT
    IF X <= 2
        SCREENLEFT
        LET X = 230
        EXIT
    ELSE
        IF CANGOLEFT
            LET IMAGE = 0
            IF T = 1
                ANIMATE
            ENDIF
            SPRITELEFT
        ENDIF
    ENDIF
ENDIF
IF KEY UP
    TABLEJUMP
    BEEP 25
ENDIF
IF SCREEN = 1
    TABLEJUMP
    BEEP 20
    ANIMATE
ENDIF
IF DEADLY
    KILL
ENDIF


EVENT SPRITETYPE1
IF SETTINGA = 0
    IF CANGOLEFT
        LET IMAGE = 2
        SPRITELEFT
        SUBTRACT 16 FROM X
        IF CANGODOWN
            LET SETTINGA = 1
        ENDIF
        ADD 16 TO X
    ELSE
        LET SETTINGA = 1
    ENDIF
ELSE
    IF CANGORIGHT
        LET IMAGE = 3
        SPRITERIGHT
        ADD 16 TO X
        IF CANGODOWN
            LET SETTINGA = 0
        ENDIF
        SUBTRACT 16 FROM X
    ELSE
        LET SETTINGA = 0
    ENDIF
ENDIF
IF T = 1
    ANIMATE
ENDIF
IF COLLISION 0
    KILL
ENDIF


