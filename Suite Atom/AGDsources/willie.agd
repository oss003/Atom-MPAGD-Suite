EVENT SPRITETYPE6

IF DIRECTION = 0            ; INITIALISE THE DRIP
   LET SETTINGB = Y         ; store initial y value in SETTINGB
   LET DIRECTION = 1        ; prevent this running again
ENDIF

IF DIRECTION = 1            ; GROW THE DRIP
   ADD 1 TO SETTINGA        ; we use SA as the timer
   IF SETTINGA = 10         ; after 2/5ths of a second
      ADD 1 TO FRAME        ; increment the frame
      LET SETTINGA = 0      ; reset the timer
      IF FRAME = 3          ; have we reached frame 3?
         LET DIRECTION = 2  ; if so goto the drop routine
      ENDIF
   ENDIF
ENDIF


IF DIRECTION = 2            ; DROP ACID!

   IF CANGODOWN             ; if we haven't hit the ground 
      FALL                  ; drop down 
   ELSE                     ; if we've hit the ground
      LET DIRECTION = 3     ; start the HIT THE GROUND routine
      ADD 1 TO FRAME        ; but increment the frame first
   ENDIF
ENDIF
   
IF DIRECTION = 3            ; HIT THE GROUND
   ADD 1 TO SETTINGA        ; increment the timer
   IF SETTINGA = 5          ; every 1/5th of a second
      ADD 1 TO FRAME        ; increment the frame
      LET SETTINGA = 0      ; reset the timer
      IF FRAME = 6          ; have we reached the end of the splash animation?
         LET FRAME = 0      ; if so reset to the first frame
	 LET Y = SETTINGB   ; reset the drop
	 LET DIRECTION = 0  ; start the INITALISE DRIP routine
      ENDIF
   ENDIF
ENDIF

