EVENT PLAYER
SPRITEINK 7
LET A Y
LET B X
IF KEY RIGHT
    IF CANGORIGHT
        SPRITERIGHT
        IF C = 2
            ANIMATE
        ENDIF
    ENDIF
    LET IMAGE 1
ENDIF
IF KEY LEFT
    IF CANGOLEFT
        SPRITELEFT
        IF C = 2
            ANIMATE
        ENDIF
        LET IMAGE 0
    ENDIF
ENDIF
IF KEY DOWN
    IF CANGODOWN
        SPRITEDOWN
        IF C = 1
            ANIMATE
        ENDIF
        LET IMAGE 3
    ENDIF
ENDIF
IF KEY UP
    IF CANGOUP
        SPRITEUP
        IF C = 1
            ANIMATE
        ENDIF
        LET IMAGE 2
    ENDIF
ENDIF
IF 12 > X
    SCREENLEFT
    LET X 148
ENDIF
IF X > 148
    SCREENRIGHT
    LET X 12
ENDIF
IF 12 > Y
    SCREENUP
    LET Y 148
ENDIF
IF Y > 148
    SCREENDOWN
    LET Y 12
ENDIF
IF F > 0
    SUBTRACT 1 TO F
ENDIF


EVENT SPRITETYPE1
SPRITEINK 7
IF Y = 200
    IF KEY FIRE
        IF F = 0
            LET F 5
            BEEP 10
            LET Y = A
            LET X = B
            LET SETTINGB = 150
            IF COLLISION 0
                OTHER
                LET Q = IMAGE
                ENDSPRITE
                LET SETTINGA = 2
                LET DIRECTION = 2
                IF 2 > Q
                    LET SETTINGA Q
                ELSE
                    LET DIRECTION Q
                    ADD 254 TO DIRECTION
                    IF KEY RIGHT
                        LET SETTINGA = 1
                    ENDIF
                    IF KEY LEFT
                        LET SETTINGA = 0
                    ENDIF
                ENDIF
            ENDIF
        ENDIF
    ENDIF
ELSE
    IF SETTINGB > 0
        SUBTRACT 1 TO SETTINGB
    ENDIF
    IF SETTINGB = 0
        LET Y 200
        EXIT
    ENDIF
    IF COLLISION 0
        IF 140 > SETTINGB
            LET Y 200
            EXIT
        ENDIF
    ENDIF
    IF SETTINGA = 0
        REPEAT 2
            IF X > 24
                IF CANGOLEFT
                    SPRITELEFT
                ELSE
                    LET SETTINGA = 1
                ENDIF
            ELSE
                LET SETTINGA = 1
            ENDIF
        ENDREPEAT
    ENDIF
    IF SETTINGA = 1
        REPEAT 2
            IF CANGORIGHT
                SPRITERIGHT
            ELSE
                LET SETTINGA = 0
            ENDIF
        ENDREPEAT
    ENDIF
    IF DIRECTION = 0
        REPEAT 2
            IF CANGOUP
                SPRITEUP
            ELSE
                LET DIRECTION = 1
            ENDIF
        ENDREPEAT
    ENDIF
    IF DIRECTION = 1
        REPEAT 2
            IF CANGODOWN
                SPRITEDOWN
            ELSE
                LET DIRECTION = 0
            ENDIF
        ENDREPEAT
    ENDIF
    IF C = 0
        ANIMATE
    ENDIF
ENDIF


EVENT SPRITETYPE2
SPRITEINK 7
IF C = 2
    ANIMATE
ENDIF
IF Y = 200
    GETRANDOM 150
    IF RND = 1
        LET IMAGE 5
        ADD 6 TO RND
        GETRANDOM 52
        MULTIPLY RND BY 2
        LET X RND
        ADD 24 TO X
        GETRANDOM 52
        MULTIPLY RND BY 2
        LET Y RND
        ADD 24 TO Y
        LET SETTINGB = 40
        IF COLLISION 2
            LET Y 200
        ENDIF
        EXIT
    ENDIF
ELSE
    IF SETTINGB > 0
        GETRANDOM 6
        ADD 1 TO RND
        IF IMAGE = 5
            SUBTRACT 1 TO SETTINGB
            SPRITEINK RND
        ENDIF
        IF IMAGE = 6
            SUBTRACT 1 TO SETTINGB
            SPRITEINK RND
        ENDIF
    ENDIF
    IF SETTINGB = 1
        GETRANDOM 3
        LET IMAGE RND
        GETRANDOM 1
        LET SETTINGA = RND
        GETRANDOM 1
        LET DIRECTION = RND
        ADD 7 TO IMAGE
        LET SETTINGB = 0
    ENDIF
    IF IMAGE > 6
        IF 10 > IMAGE
            IF COLLISION 1
                OTHER
                LET X 0
                LET Y 200
                ENDSPRITE
                LET SETTINGB = 200
                LET IMAGE 6
                LET FRAME 0
                EXIT
            ENDIF
            SUBTRACT 4 TO IMAGE
            SPRITEINK IMAGE
            ADD 4 TO IMAGE
        ENDIF
    ENDIF
    IF SETTINGB > 175
        ANIMATE
        IF IMAGE = 6
            BEEP SETTINGB
        ENDIF
    ENDIF
    IF SETTINGB = 175
        LET Y 200
        LET X 150
        LET SETTINGB = 100
        LET FRAME 0
    ENDIF
    IF SETTINGB = 0
        IF SETTINGA = 0
            IF 135 > X
                SPRITERIGHT
            ELSE
                LET SETTINGA = 1
            ENDIF
        ELSE
            IF X > 25
                SPRITELEFT
            ELSE
                LET SETTINGA = 0
            ENDIF
        ENDIF
        IF DIRECTION = 0
            IF Y > 25
                SPRITEUP
            ELSE
                LET DIRECTION = 1
            ENDIF
        ELSE
            IF 135 > Y
                SPRITEDOWN
            ELSE
                LET DIRECTION = 0
            ENDIF
        ENDIF
    ENDIF
ENDIF


