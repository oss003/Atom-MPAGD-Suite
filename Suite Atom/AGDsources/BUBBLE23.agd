IF KEY FIRE                     ; Si pulso el boton de disparo ...
   IF D = 0                        ; si D = 0 es porque he soltado el boton de disparo desde la ultima vez.
      BEEP 50                                
      IF F < 12                     ;  hay menos de 6 burbujas
			IF DIRECTION = LEFT       ; si mira el dragon hacia la izquierda
				SUBTRACT 16 TO X       ; set X 32 pixels a la derecha
				SPAWN 2 4              ; SPAWN burbuja (TYPE 2, IMAGE 4)
				ADD 16 FROM X          ; reset X a su posicion original
				ADD 1 TO F             ; suma una burbuja al contador
				LET D = 1              ; si D = 1 todavia no he soltado el boton de disparo
			ELSE
				IF DIRECTION = RIGHT   ; si el dragon mira hacia la izquierda
					ADD 16 FROM X       ; set X 32 pixels a la izquierda
					SPAWN 2 4           ; SPAWN burbuja (TYPE 2, IMAGE 4)
					SUBTRACT 16 TO X    ; reset X a su posicion original
					ADD 1 TO F          ; suma una burbuja al contador
		 			LET D = 1           ; si D = 1 todavia no he soltado el boton de disparo  
				ENDIF
      	ENDIF               
   	ENDIF
	ENDIF
ELSE      									; si he soltado el boton de disparar
