EVENT PLAYER ; dragon
LET V = X
LET W = Y
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
IF S = 0  ;;;;;;rutina que hace aparecer la MAMA, cuando los segundos llegan a 0   
	BEEP 50
	AT 7 11
	PRINTMODE 1
	MESSAGE "MOVE FASTER!!" 
	DELAY 100    
	REDRAW
	IF DIRECTION = RIGHT    ; si mira el dragon hacia la izquierda
		LET V = X
		LET W = Y
		LET X = 32
		LET Y = 32
		SPAWN 8 12              ; SPAWN MAMA (TYPE 2, IMAGE 4)
		ADD 20 TO S
		LET X = V
		LET Y = W
	ELSE
		IF DIRECTION = LEFT   ; si el dragon mira hacia la izquierda
			LET V = X
			LET W = Y
			LET X = 32
			LET Y = 32
			SPAWN 8 12           ; SPAWN MAMA (TYPE 2, IMAGE 4)
			ADD 20 TO S
			LET X = V
			LET Y = W
		ENDIF
	ENDIF
ENDIF
IF KEY LEFT
    LET DIRECTION = LEFT
    LET IMAGE = 2
    IF X <= LEFTEDGE
        SCREENLEFT
        LET X = RIGHTEDGE
        EXIT
    ELSE
        IF CANGOLEFT
            SPRITELEFT
            ANIMATE SLOW
        ENDIF
    ENDIF
ENDIF
IF KEY RIGHT
    LET DIRECTION = RIGHT
    LET IMAGE = 0
    IF X >= RIGHTEDGE
        SCREENRIGHT
        LET X = LEFTEDGE
        EXIT
    ELSE
        IF CANGORIGHT
            SPRITERIGHT
            ANIMATE SLOW
        ENDIF
    ENDIF
ENDIF
IF KEY UP
	;JUMP 8
	TABLEJUMP
	IF AIRBORNE 1 ; si no esta en el aire, suena el beep
   	BEEP 100
	ENDIF
ELSE
	IF CANGODOWN
		IF Y >= BOTTOMEDGE
			LET Y = TOPEDGE
			SCREENDOWN
		ELSE
			SPRITEDOWN
			ANIMATE
		ENDIF
		FALL SLOW
	ENDIF
ENDIF

