EVENT SPRITETYPE2 ; sprite burbuja
IF SETTINGA < 10
   ADD 1 TO SETTINGA
	IF DIRECTION = LEFT
       IF CANGOLEFT
			 SPRITELEFT                 ;
			 SPRITELEFT                 ;
		 ENDIF
	ELSE                                ; la burbuja se mueve 1 segundo
       LET DIRECTION = RIGHT      ; a derecha o izquierda
		 IF CANGORIGHT
	       SPRITERIGHT                ; segun este mirando el dragon
			 SPRITERIGHT 
		 ENDIF  
	ENDIF
; dentro de esta rutina si la burbuja choca con el enemigo lo mete en al burbuja
; si no impacta, la burbuja pasa a la rutina boncing nasty
   IF COLLISION SPRITETYPE1 ; si la burbuja colisiona con el enemigo dentro del segundo del marcador.
	   REMOVE ; elimino la burbuja
	   SUBTRACT 1 TO F ; descuento una burbuja del contador
		OTHER ; paso al spritetype1
		   REMOVE ; lo borro
		ENDSPRITE ; termino con el spritetype1
	   SPAWN 3 5 ; pongo el spritetype3 en su lugar (enemigo dentro de la burbuja)
   ENDIF
	IF COLLISION SPRITETYPE4 ; si la burbuja colisiona con el enemigo dentro del segundo del marcador.
		REMOVE ; elimino la burbuja
		SUBTRACT 1 TO F ; descuento una burbuja del contador
		OTHER ; paso al spritetype5
			REMOVE ; lo borro
		ENDSPRITE ; termino con el spritetype5
		SPAWN 5 8 ; pongo el spritetype4 en su lugar (enemigo dentro de la burbuja)
	ENDIF
	IF COLLISION SPRITETYPE6 ; si la burbuja colisiona con el enemigo dentro del segundo del marcador.
		REMOVE ; elimino la burbuja
		SUBTRACT 1 TO F ; descuento una burbuja del contador
		OTHER ; paso al spritetype5
			REMOVE ; lo borro
		ENDSPRITE ; termino con el spritetype5
		SPAWN 7 11 ; pongo el spritetype7 en su lugar (enemigo dentro de la burbuja)
	ENDIF
ELSE
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; rutina bouncing nasty
	IF SETTINGB = LEFT                             ; determina izquierda y derecha
		IF CANGOLEFT
			 LET IMAGE = 4
			 SPRITELEFT
		ELSE
			 LET SETTINGB = RIGHT
			 LET FRAME = 0
		ENDIF
	ELSE
		IF CANGORIGHT
			 LET IMAGE = 4
			 SPRITERIGHT
		ELSE
			 LET SETTINGB = LEFT
			 LET FRAME = 0
		ENDIF
		IF F = 6        ; para evitar dibujar mas de 6 burbujas
			REMOVE
			SUBTRACT 1 TO F
		ENDIF
	ENDIF
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; determina arriba y abajo
	IF DIRECTION = UP                             ;
		IF CANGOUP                ;;;;;;;;;;
			LET IMAGE = 4                  ;
			SPRITEUP                       ;
		ELSE                               ;
			LET DIRECTION = DOWN           ;
			LET FRAME = 0                  ;
		ENDIF                     ;;;;;;;;;;
	ELSE                                     ;;;;;;;;;;;;;
		IF CANGODOWN              ;;;;;;;;;;
			LET IMAGE = 4                  ;
			SPRITEDOWN                     ;
		ELSE                               ;
			LET DIRECTION = UP             ;
			LET FRAME = 0                  ; 
		ENDIF                     ;;;;;;;;;;
		IF F = 6        ; para evitar dibujar mas de 6 burbujas
			REMOVE
			SUBTRACT 1 TO F
		ENDIF
	ENDIF
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; endif de la primera linea, contador;;
;;;;;;;;;;;;;;;;;;;;;rutina que controla si las burbujas salen de la pantalla
	IF Y <= TOPEDGE             ; si la burbuja llega a la parte superior
		REMOVE                   ; eliminamos burbuja
		SUBTRACT 1 TO F          ; restamos un burbuja al marcador de burbujas
	ELSE                        ; si no sucede esto, pero sucede por la parte de abajo
		IF Y > BOTTOMEDGE   ; si esta en la parte de abajo
			REMOVE              ; eliminar burbuja
			SUBTRACT 1 TO F     ; restar una burbuja del marcador de burbujas
		ENDIF	            
	ENDIF
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
	IF KEY UP        ;;;;;;;; si el player pulsa el boton del salto, salta sobre la burbuja.
		IF COLLISION 0
			LET A = Y
			OTHER
				IF A > Y
					SUBTRACT Y FROM A
					IF A > 8
						ADD Y TO A
						LET Y = A
						SUBTRACT 16 FROM Y
						STOPFALL
					ENDIF
					IF KEY FIRE
						TABLEJUMP
					ENDIF
				ENDIF
			ENDSPRITE
		ENDIF
	ENDIF
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
	IF COLLISION PLAYER   ;;;;;;; si colisiona con el jugador , quito burbuja y descuento burbuja del contador
		REMOVE
		SUBTRACT 1 TO F
		SCORE 25
		AT 1 13
		PRINTMODE 0
		SHOWSCORE
	ENDIF
ENDIF


