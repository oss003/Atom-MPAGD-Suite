EVENT SPRITETYPE6



;======================================PUSHABLE CRATES 
IF IMAGE 27                         ; image 5 is my pushable crate
   ;SPRITEINK 6
   FALL                            ; let gravity do its thing
  

   IF COLLISION PLAYER             ; is the player touching the crate?
      IF X < V                     ; is crate on left of player?
         LET SETTINGA = LEFT       ; if so we will push it left
      ELSE                         ; if not
         LET SETTINGA = RIGHT      ; crate is on the right so we'll push it right
      ENDIF
	  
      IF W > Y                     ; is player lower than the crate?
         IF SETTINGA = LEFT        ; and if crate is on the left    
            OTHER                  ; push fails so move the player back to the right
            UNDOSPRITEMOVE
            ENDSPRITE
         ELSE
            IF SETTINGA = RIGHT    ; and if crate is on the right
               OTHER               ; push fails so move player to the left
               UNDOSPRITEMOVE
               ENDSPRITE
            ENDIF
         ENDIF                        

      ENDIF

      IF Y > W                     ; if player is higher than the crate
         SUBTRACT W FROM Y         ; how much higher is the player?
         IF Y >= 9                 ; if it is 9 or more pixels         
            OTHER                  ; switch to the player
            IF CANGOUP             ; if there is space     
               STOPFALL            ; stop the player on the crate
               SUBTRACT 1 FROM Y   ; required to prevent player/crate overlapping
            ENDIF
            ENDSPRITE       
         ENDIF
	 ADD W TO Y                ; reset the crates Y value
      ELSE                         ; if player less than 9px above, landing fails
         IF SETTINGA = LEFT        ; if crate is on left  
            OTHER                  ; push has failed so move player to the right
            UNDOSPRITEMOVE
            ENDSPRITE
         ELSE                      ; else crate is on right
            OTHER                  ; push has failed so move player to the left
            UNDOSPRITEMOVE
            ENDSPRITE
         ENDIF
      ENDIF
  

      IF W = Y                     ; if player is level with the crate push success!
         IF SETTINGA = LEFT        ; if crate is on the left of player
            IF CANGOLEFT           ; and there is room to the left
               SPRITELEFT          ; push the crate left
            ELSE                   ; but if there is no room to push the crate
               OTHER               ; switch to the player
               IF CANGORIGHT       ; if there is space to the right
                  SPRITERIGHT      ; move the player right
               ENDIF
               ENDSPRITE
            ENDIF
         ENDIF
         IF SETTINGA = RIGHT       ; if crate is on the right of player
            IF CANGORIGHT          ; and there is room to the right
               SPRITERIGHT         ; push the crate right
            ELSE                   ; but if there is no room to push the crate
               OTHER               ; switch to the player
               IF CANGOLEFT        ; if there is space to the left
                  SPRITELEFT       ; move the player left
               ENDIF
               ENDSPRITE
            ENDIF
         ENDIF
      ENDIF
   ENDIF
ENDIF
