EVENT PLAYER
LET A = Y
LET B = X
LET O DIRECTION
LET P DIRECTION
DIVIDE O BY 16
MULTIPLY O BY 16
SUBTRACT O FROM P
DIVIDE O BY 16
IF 8 > O
    ADD O TO Y
ELSE
    SUBTRACT O FROM Y
    ADD 8 TO Y
ENDIF
IF 8 > P
    ADD P TO X
ELSE
    SUBTRACT P FROM X
    ADD 8 TO X
ENDIF
IF SCREEN > 0
    IF KEY DOWN
        IF 4 > O
            ADD 1 TO O
        ENDIF
        IF O > 8
            SUBTRACT 1 FROM O
        ENDIF
    ENDIF
    IF KEY UP
        IF O > 8
            IF 12 > O
                ADD 1 TO O
            ENDIF
        ENDIF
        IF O = 0
            LET O = 9
        ENDIF
        IF 5 > O
            SUBTRACT 1 FROM O
        ENDIF
    ENDIF
    IF KEY RIGHT
        IF 4 > P
            ADD 1 TO P
        ENDIF
        IF P > 8
            SUBTRACT 1 FROM P
        ENDIF
    ENDIF
    IF KEY LEFT
        IF P > 8
            IF 12 > P
                ADD 1 TO P
            ENDIF
        ENDIF
        IF P = 0
            LET P = 9
        ENDIF
        IF 5 > P
            SUBTRACT 1 FROM P
        ENDIF
    ENDIF
    IF O = 8
        LET O = 0
    ENDIF
    IF P = 8
        LET P = 0
    ENDIF
ENDIF
IF KEY RIGHT
ELSE
    IF KEY LEFT
    ELSE
        IF P > 0
            SUBTRACT 1 FROM P
            IF P = 8
                LET P = 0
            ENDIF
        ENDIF
    ENDIF
ENDIF
IF KEY DOWN
ELSE
    IF KEY UP
    ELSE
        IF O > 0
            SUBTRACT 1 FROM O
            IF O = 8
                LET O = 0
            ENDIF
        ENDIF
    ENDIF
ENDIF
