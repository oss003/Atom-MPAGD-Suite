EVENT PLAYER
IF IMAGE = 0
    LET A Y
    LET B X
ENDIF
SUBTRACT 8 TO Y
; SPRITEINK 7
ADD 8 TO Y
IF COLLISION 10
    BEEP 150
ENDIF
IF IMAGE = 2
    IF KEY DOWN
        IF 8 > FRAME
            ADD 1 TO FRAME
        ENDIF
    ENDIF
    IF KEY UP
        IF FRAME > 0
            SUBTRACT 1 TO FRAME
        ENDIF
    ENDIF
ELSE
    IF KEY LEFT
        ANIMBACK
    ELSE
        ANIMATE
    ENDIF
ENDIF
IF KEY RIGHT
    SPRITERIGHT
ENDIF
IF KEY LEFT
    SPRITELEFT
ENDIF
IF KEY FIRE
    IF IMAGE = 2
        ADD 8 TO X
        LET RND FRAME
        SUBTRACT 4 TO RND
        IF RND > 250
            ADD 16 TO RND
        ENDIF
        IF C = 0
            IF KEY UP
                SUBTRACT 1 TO RND
            ENDIF
            LASER RND
        ENDIF
        SUBTRACT 8 TO X
    ENDIF
ENDIF
IF 2 > IMAGE
    ; SPRITEINK 6
    ADD 8 TO Y
    ; SPRITEINK 5
    SUBTRACT 16 TO Y
    ; SPRITEINK 2
    ADD 8 TO Y
ENDIF


