ENDIF
IF COLLISION 10
    BEEP 10
    ADD 1 TO DIRECTION
    ; SPRITEINK 2
    IF DIRECTION = 4
        LET IMAGE 3
        LET FRAME 0
    ENDIF
ELSE
    IF IMAGE <> 3
        LET OPT 6
        LET RND 5
        IF IMAGE = 4
            LET OPT 5
            LET RND 3
        ENDIF
        ADD 8 TO Y
        ; SPRITEINK OPT
        SUBTRACT 8 TO Y
        ; SPRITEINK RND
    ELSE
        LET RND 0
        IF C = 0
            LET RND 1
        ENDIF
        IF C = 2
            LET RND 1
        ENDIF
        IF RND = 1
            ADD 1 TO FRAME
            IF FRAME = 4
                REMOVE
                EXIT
            ENDIF
        ENDIF
    ENDIF
ENDIF


