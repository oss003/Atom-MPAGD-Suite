    DIG UP
    TABLEJUMP
    IF B = 1
        IF G = 0
            LET IMAGE = 10
        ENDIF
        IF G = 1
            LET IMAGE = 11
        ENDIF
    ENDIF
ENDIF
IF CANGODOWN
    DIG DOWN
    IF LADDERBELOW
        DIG DOWN
    ELSE
        IF DEADLY
        ELSE
            TABLEFALL
            IF B = 1
                IF G = 0
                    LET IMAGE = 10
                ENDIF
                IF G = 1
                    LET IMAGE = 11
                ENDIF
            ENDIF
            DIG DOWN
        ENDIF
    ENDIF
ENDIF
IF KEY UP
    DIG UP
    IF LADDERABOVE
        IF B = 0
            LET IMAGE = 2
            ANIMATE
        ENDIF
        IF B = 1
            LET IMAGE = 12
            ANIMATE
        ENDIF
        SPRITEUP
    ENDIF
ENDIF
IF KEY DOWN
    DIG DOWN
    IF LADDERBELOW
        DIG DOWN
        IF B = 0
            LET IMAGE = 2
            ANIMATE
        ENDIF
        IF B = 1
            LET IMAGE = 12
            ANIMATE
        ENDIF
        SPRITEDOWN
    ENDIF
ENDIF
IF KEY RIGHT
    DIG RIGHT
    IF X > 239
        SCREENRIGHT
        LET X = 239
    ELSE
        IF CANGORIGHT
            IF B = 0
                LET IMAGE = 0
                ANIMATE
            ENDIF
            IF B = 1
                LET IMAGE = 10
                ANIMATE
            ENDIF
            SPRITERIGHT
            LET G = 0
            DIG RIGHT
            IF CUSTOM
                LET C = Y
                LET D = X
                DIVIDE C BY 8
                DIVIDE D BY 8
                ADD 2 TO C
                ADD 2 TO D
                LET LINE = C
                LET COLUMN = D
                PUTBLOCK 4
                SUBTRACT 2 FROM C
                ADD 0 TO D
                LET LINE = C
                LET COLUMN = D
                PUTBLOCK 6
                BEEP 25
                SCORE 25
                LET LINE = 0
                LET COLUMN = 25
                SHOWSCORE
                ADD 1 TO A
            ENDIF
        ENDIF
    ENDIF
ENDIF
; warning - ENDIF without IF?
ENDIF
IF KEY LEFT
    DIG LEFT
    IF X <= 1
        SCREENLEFT
        LET X = 1
    ELSE
        IF CANGOLEFT
            IF B = 0
                LET IMAGE = 1
                ANIMATE
            ENDIF
            IF B = 1
                LET IMAGE = 11
                ANIMATE
            ENDIF
            SPRITELEFT
            LET G = 1
            DIG LEFT
            IF CUSTOM
                LET C = Y
                LET D = X
                DIVIDE C BY 8
                DIVIDE D BY 8
                ADD 2 TO C
                ADD 0 TO D
                LET LINE = C
                LET COLUMN = D
                PUTBLOCK 4
                SUBTRACT 2 FROM C
                ADD 0 TO D
                LET LINE = C
                LET COLUMN = D
                PUTBLOCK 6
                BEEP 25
                SCORE 25
                LET LINE = 0
                LET COLUMN = 25
                SHOWSCORE
                ADD 1 TO A
            ENDIF
        ENDIF
    ENDIF
ENDIF
; warning - ENDIF without IF?
ENDIF


