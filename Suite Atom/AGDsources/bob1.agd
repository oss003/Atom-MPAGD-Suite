                SUBTRACT 2 FROM X
                LET SETTINGB 1
                LET DIRECTION 1
            ENDIF
        ENDIF
        IF Y <> 88
            ; SPRITEINK 70
        ENDIF
        IF SETTINGB = 0
            LET IMAGE 4
        ENDIF
    ENDIF
    LET O Y
    LET P X
    LET E DIRECTION
    IF I = 0
