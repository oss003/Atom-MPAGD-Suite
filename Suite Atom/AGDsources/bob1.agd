                            ; COLOUR 2
                        ENDIF
                    ENDIF
                    IF N = 77
                        ; COLOUR 6
                    ENDIF
                    IF N = 78
                        ; COLOUR 5
                    ENDIF
                    MESSAGE 8
                    LET M 1
                    LET X 224
                    LET Y 176
                    PUT X Y N
                ELSE
                    LET LINE 15
                    LET COLUMN 15
                    IF N = 73
                        ; COLOUR 4
                    ENDIF
                    IF N > 73
