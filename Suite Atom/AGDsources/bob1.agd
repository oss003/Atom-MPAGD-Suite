                ENDIF
            ENDIF
        ENDIF
    ELSE
        IF M > 0
            ADD 1 TO M
            IF M = 255
                IF N = 0
                    IF L = 1
                        LET N 73
                    ENDIF
                    IF L = 2
                        LET N 74
                    ENDIF
                    IF L > 2
                        IF L <= 4
