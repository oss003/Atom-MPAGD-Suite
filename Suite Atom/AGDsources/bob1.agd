                            ; COLOUR 2
                        ENDIF
                    ENDIF
                    IF N = 77
                        ; COLOUR 6
                    ENDIF
                    IF N = 78
                        ; COLOUR 5
                    ENDIF
                    MESSAGE 8
                    LET LINE 16
                    LET COLUMN 15
                    IF N > 72
                        IF N <= 76
                            ; COLOUR 2
                        ENDIF
                    ENDIF
                    IF N = 77
                        ; COLOUR 6
                    ENDIF
                    IF N = 78
                        ; COLOUR 5
                    ENDIF
                    MESSAGE 8
                    LET M 0
                    GET N
                    LET Y 120
                    LET X 120
                    PUT X Y N
                    LET N 0
                ENDIF
                LET Y O
                LET X P
            ENDIF
        ENDIF
        DETECTOBJ
        IF OBJ <> 255
            GET OBJ
            IF OBJ <= 72
                IF OBJ <= 3
                    BEEP 100
                    LET B 1
                    LET D 128
                    SCORE 5
                ELSE
                    BEEP 20
