                            ; COLOUR 2
                        ENDIF
                    ENDIF
                    IF N = 77
                        ; COLOUR 6
                    ENDIF
                    IF N = 78
                        ; COLOUR 5
                    ENDIF
                    MESSAGE 8
                    LET LINE 23
