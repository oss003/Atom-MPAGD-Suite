        LET I 1
      ENDIF
    ENDIF

    IF I 1
      AT 23,1
      PUTBLOCK 2			; Wipe object in possesion
      LET W 128
    ENDIF

  ENDIF

;----------------------------------------------------------------------
; Check Mario-style jump on top
; Strength is copied to stumbled sprite type 8
;----------------------------------------------------------------------

  LET L 2				; Child/spider
  LET M 11				; Set image
  REPEAT 3
    IF L 4				; Child
      LET M 17				; Set image
    ENDIF
    IF L 5				; Spider
      LET M 5				; Set image
    ENDIF
    IF COLLISION L			; Check hit enemy
      OTHER
        LET I AIRBORNE
        LET J Y
        LET K SETTINGA
      ENDSPRITE
      IF I 0				; If enemy not in air
        IF AIRBORNE > 0			; If in air
          IF JUMPSPEED < 128  		; If juming down
            IF Y < J			; If hitting top
              STOPFALL
              BEEP 30
              JUMP 4			; Jump
              OTHER
                REMOVE			; Remove enemy
                SPAWN 8 M		; Spaw stumbled enemy
                SPAWNED
                  LET SETTINGA K	; Transfer strength
                ENDSPRITE
              ENDSPRITE
            ENDIF
          ENDIF
        ENDIF
      ENDIF
    ENDIF
    IF L 4				; If child, next check spider
      LET L 5
    ENDIF
    IF L 2
      LET L 4				; If spiderchild, next check child
    ENDIF
  ENDREPEAT

;----------------------------------------------------------------------
; Invulnerable
;----------------------------------------------------------------------

  IF H > 0
    SUBTRACT 1 FROM H
    SPRITEINK H			; Blink player
    EXIT				; Needed to avoid colour attribute clash
  ENDIF

;----------------------------------------------------------------------
; Check if player is eating worm
;----------------------------------------------------------------------

  LET I F				; Save fase
  DIVIDE I BY 6
  ADD 20 TO I
  IF IMAGE I				; Check if image is eating
    SUBTRACT 1 FROM SETTINGB		; Subtract counter
    IF SETTINGB 0			; If counter 0 -> stand
      LET IMAGE F			; Stand left
    ELSE
      ANIMATE SLOW			; If counter>0 -> animate
      GETRANDOM 30
      MULTIPLY RND 2
      ADD 10 TO RND
      BEEP RND
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check screen leaving left
;----------------------------------------------------------------------

  IF DIRECTION LEFT			; If moving left
    IF X <= LEFTEDGE			; If crossing left border
      SCREENLEFT			; Screen left
;	USER 1				; Scroll right
      LET X 230				; Set X-pos
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check screen leaving right
;----------------------------------------------------------------------

  IF DIRECTION RIGHT			; If moving right
    IF X >= RIGHTEDGE			; If crossing right border
      SCREENRIGHT			; Screen right
;	USER 0				; Scroll left
      LET X 10				; Set X-pos
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check screen leaving up
;----------------------------------------------------------------------

  IF Y <= 9				; If crossing top border
    LET Y 150				; Set Y-pos
    SCREENUP				; Screen up
;    USER 3				; Scroll down
    EXIT
  ENDIF

;----------------------------------------------------------------------
; Check screen leaving down
;----------------------------------------------------------------------

  IF Y >=160				; If crossing bottom border
    LET Y 10				; Set Y-pos
    SCREENDOWN				; Screen down
;    USER 2				; Scroll up
    EXIT
  ENDIF

;----------------------------------------------------------------------
; Check actions
;----------------------------------------------------------------------

  LET I 0				; Reset move flag

; Check move left

  IF KEY LEFT				; Check pressed left
    LET DIRECTION = LEFT
    LET IMAGE = F
    ADD 4 TO IMAGE			; Walk left
    IF CANGOLEFT
      SPRITELEFT
    ENDIF
    LET I 1
  ENDIF

; Check move right

  IF KEY RIGHT				; Check pressed right
    LET DIRECTION = RIGHT
    LET IMAGE = F
    ADD 2 TO IMAGE			; Walk right
    IF CANGORIGHT
      SPRITERIGHT 
    ENDIF
    LET I 1
  ENDIF

; Check no moving = standing

  IF I = 0				; Display standing player
    IF N 0
      LET IMAGE F
      LET FRAME 0 
      DIVIDE X BY 8			; Align player to block
      MULTIPLY X BY 8
    ENDIF
  ELSE
    ANIMATE
  ENDIF

; Check if jumping

  IF AIRBORNE > 0			; Display jumping player
    LET IMAGE F
    IF DIRECTION = LEFT
      ADD 4 TO IMAGE			; Jump left up
      LET FRAME 0
    ELSE
      ADD 2 TO IMAGE			; Jump right up
      LET FRAME 0
    ENDIF
    IF JUMPSPEED < 128
      LET FRAME	1			; Jump down
    ENDIF
  ENDIF

; Check for jump key

  IF KEY UP				; Check for jumping
    IF AIRBORNE = 0
      BEEP 50
    ENDIF
    JUMP V
  ENDIF

; Check for spit

  IF KEY FIRE				; Spit
    LET I X
    IF N = 0
      LET IMAGE F
      IF DIRECTION LEFT 
        SUBTRACT 16 FROM X
        ADD 3 TO IMAGE			; Spit left
      ELSE
        ADD 16 TO X
        ADD 1 TO IMAGE			; Spit right
      ENDIF 
      BEEP 80
      SPAWN 3 18			; Spawn spit left
      SPAWNED
        IF F 0
          LET SETTINGB 2
        ELSE
          IF F 6
            LET SETTINGB 2
          ELSE
            LET SETTINGB 3
          ENDIF
        ENDIF
      ENDSPRITE
      LET SETTINGA 8
      LET FRAME 0 
      LET N 20				; Shoot delay
    ENDIF
    LET X I
  ENDIF

  LET O X				; Save X-pos player
  LET P Y				; Save Y-pos player

;----------------------------------------------------------------------
; Adjust shoot delay
;----------------------------------------------------------------------

  IF N > 0				; Shoot delay
    SUBTRACT 1 FROM N
  ENDIF

  FALL					; Gravity

;----------------------------------------------------------------------
; ENEMY WORM
;----------------------------------------------------------------------
; SETTINGA = Animation delay counter
; SETTINGB = Delay counter
;----------------------------------------------------------------------

EVENT SPRITETYPE1

;----------------------------------------------------------------------
; Check for fall off screen
;----------------------------------------------------------------------

  IF Y 160
    REMOVE				; At bottom remove
    EXIT
  ENDIF

;----------------------------------------------------------------------
; Look ahead 15 pixels
;----------------------------------------------------------------------

  IF SETTINGA = 0			; If no delay

    LET I X				; Save X-pos

    IF DIRECTION RIGHT			; Check ahead
      ADD 15 TO X			; Look right
      LET J LEFT
    ELSE
      SUBTRACT 15 FROM X		; Look left
      LET J RIGHT
    ENDIF

    IF COLLISION 0			; Player
      LET DIRECTION J			; If so, reverse direction
      LET SETTINGA 25			; Set delay 1/2 sec
    ELSE
      IF COLLISION 2			; Child/Spider
        LET DIRECTION J			; If so, reverse direction
        LET SETTINGA 25			; Set delay 1/2 sec
      ELSE
        IF COLLISION 4			; Child
          LET DIRECTION J		; If so, reverse direction
          LET SETTINGA 25		; Set delay 1/2 sec
        ELSE
          IF COLLISION 5		; Baby spider
            LET DIRECTION J		; If so, reverse direction
            LET SETTINGA 25		; Set delay 1/2 sec
          ENDIF
        ENDIF
      ENDIF
    ENDIF
    LET X I				; Restore X-pos
  ENDIF
  
;----------------------------------------------------------------------
; Check hit by player 
;----------------------------------------------------------------------

  IF H = 0
    IF COLLISION 0			; If hit by player
      IF AIRBORNE 0			; If not falling
        OTHER
          LET I JUMPSPEED
          LET J AIRBORNE
        ENDSPRITE
        IF J 0				; Don't eat falling worm
          IF I < 128			; Jump down
            REMOVE
            LET I X
            LET J Y
            OTHER
              LET X I
              LET Y J
              LET K F			; Save fase
              DIVIDE K BY 6
              LET IMAGE K		; Start eating worm animation
              ADD 20 TO IMAGE
              LET FRAME 0
              LET SETTINGB 25		; Set eating counter
            ENDSPRITE
            LET W 3			; Energy +3
            EXIT
          ENDIF
        ENDIF
      ENDIF
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check hit by spider, spiderchild, child
;----------------------------------------------------------------------

  LET K 2				; Spiderchild
  LET L 21				; Image
  LET M 11				; Spawn
  LET T 4				; Spritetype

  REPEAT 3
    IF COLLISION K			; If hit
      IF AIRBORNE 0			; If not falling
        OTHER
          LET I JUMPSPEED
          LET J AIRBORNE
        ENDSPRITE
        IF J 0				; Don't eat falling worm
          IF I < 128			; Jump down
            REMOVE
            LET I X
            LET J Y
            OTHER
              LET X I
              LET Y J
              LET IMAGE L		; Start eating worm animation
              LET FRAME 0
              LET SETTINGB 25		; Set eating counter
              IF SETTINGA < 5		; Evolve after eating 2 worms
                ADD 1 TO SETTINGA	; Strength  +1
              ELSE
                IF K <> 4
                  REMOVE
                  SPAWN T,M		; Evolve if not child
                ENDIF
              ENDIF
            ENDSPRITE
            EXIT
          ENDIF
        ENDIF
      ENDIF
    ENDIF

    IF K 4
      LET K 5				; Spider
      LET L 20				; Image
      LET M 6				; Spawn
      LET T 2				; Spritetype
    ENDIF
    IF K 2
      LET K 4				; Child
      LET L 22				; Image
    ENDIF

  ENDREPEAT

;----------------------------------------------------------------------
; Only execute when delay counter is expired
;----------------------------------------------------------------------

  IF SETTINGA = 0			; Check delay factor

    LET I X				; Save X
    LET J Y				; Save Y
    ADD 20 TO J				; Check platform

;----------------------------------------------------------------------
; Check movements
;----------------------------------------------------------------------

    IF DIRECTION LEFT			; Check going left
      IF CANGOLEFT
        ADD 4 TO I
        GETBLOCK I,J
        IF BLOCK EMPTYBLOCK		; Check for left end of platform
          LET DIRECTION RIGHT		; If so, reverse direction
          LET SETTINGA 25		; Delay 1 sec
        ELSE
          SPRITELEFT			; If not, move left
        ENDIF
      ELSE
        LET DIRECTION RIGHT		; If not cangoleft, reverse direction
        LET SETTINGA 25			; Delay 1 sec
      ENDIF
    ENDIF

    IF DIRECTION RIGHT			; Check going right
      IF CANGORIGHT
        ADD 12 TO I
        GETBLOCK I,J
        IF BLOCK EMPTYBLOCK		; Check for right end of platform
          LET DIRECTION LEFT		; If so, reverse direction
          LET SETTINGA 25		; Delay 1 sec
        ELSE
          SPRITERIGHT			; If not, move right
        ENDIF
      ELSE
        LET DIRECTION LEFT		; If not cangoright, reverse direction
        LET SETTINGA 25			; Delay 1 sec
      ENDIF
    ENDIF

    IF DIRECTION LEFT			; Determine animation direction
      ANIMATE 
    ELSE
      ANIMBACK 
    ENDIF

;----------------------------------------------------------------------
; Animation delay
;----------------------------------------------------------------------

    IF SETTINGA < 3
      LET SETTINGA 3			; Reset delaycounter
    ENDIF

  ENDIF

;----------------------------------------------------------------------
; Delay 
;----------------------------------------------------------------------

  IF SETTINGB > 0
    SUBTRACT 1 FROM SETTINGB
  ENDIF

  SUBTRACT 1 FROM SETTINGA		; Increment delaycounter
  TABLEFALL				; If off platform, fall

;----------------------------------------------------------------------
; ENEMY CHILD/SPIDER
;----------------------------------------------------------------------
; SETTINGA = Strength
; SETTINGB = Movement delay (jump/direction change)
;----------------------------------------------------------------------

EVENT SPRITETYPE2

;----------------------------------------------------------------------
; Check Mario-style jump on top
; Strength is copied to stumbled sprite type 8
;----------------------------------------------------------------------

  LET L 4				; Child
  LET M 17				; Set image
  REPEAT 3
    IF COLLISION L			; Check if hit enemy
      OTHER
       LET I AIRBORNE
        LET J Y
        LET K SETTINGA
      ENDSPRITE
      IF I 0				; If enemy not in air
        IF AIRBORNE > 0			; If in air
          IF JUMPSPEED < 128  		; If juming down
            IF Y < J			; If hitting top
              STOPFALL
              JUMP 4			; Jump
              OTHER
                REMOVE
                SPAWN 8 M		; Spawn stumbled enemy
                SPAWNED
                  LET SETTINGA K
                  IF L 0		; If player
                    LET SETTINGB 178	; Set stumbled player flag
                  ENDIF
                ENDSPRITE
              ENDSPRITE
            ENDIF
          ENDIF
        ENDIF
      ENDIF
    ENDIF
    IF L 0
      LET L 5				; Spider
      LET M 5				; Set image
    ENDIF
    IF L 4
      LET L 0				; Player
      LET M F				; Set image
      ADD 5 TO M
    ENDIF

  ENDREPEAT

;----------------------------------------------------------------------
; Remove player if falling to screen below
;----------------------------------------------------------------------

  IF Y 160
    REMOVE				; At bottom remove
    EXIT
  ENDIF

;----------------------------------------------------------------------
; Hitting blocks
;----------------------------------------------------------------------

  IF CUSTOM
    GETRANDOM 4				; Jump for about 50%
    IF RND = 1
      IF X < O				; Jump towards player
        LET DIRECTION RIGHT
      ELSE
        LET DIRECTION LEFT
      ENDIF
      IF P < Y				; Only jump if player above enemy
        JUMP 8				; If hitting custom block, jump
      ENDIF
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check if child/spider is eating worm
;----------------------------------------------------------------------

  IF IMAGE 21				; Check if image is eating
    SUBTRACT 1 FROM SETTINGB		; Subtract counter
    IF SETTINGB 0			; If counter 0 -> stand
      LET IMAGE 6			; Stand left
      LET FRAME 0
    ELSE
      ANIMATE SLOW			; If counter>0 -> animate
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check for delay, SettingB:
;       0  = No delay
; 1,11,21  = End delay
;    2-10  = Delay for jump
;   12-20  = Delay for right
;   22-30  = Delay for left
;----------------------------------------------------------------------

  IF SETTINGB > 0
    SUBTRACT 1 FROM SETTINGB
    IF SETTINGB 21			; Check waiting for going left
      LET DIRECTION LEFT
      LET SETTINGB 0
    ENDIF
    IF SETTINGB 11			; Check waiting for going right
      LET DIRECTION RIGHT
      LET SETTINGB 0
    ENDIF
    IF SETTINGB 1			; Check waiting for jumping
      GETRANDOM 2			; Jump for 50%
      IF RND 1
        JUMP 8
      ENDIF
      LET SETTINGB 0
    ENDIF
    IF AIRBORNE 0
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Show right image for rising/falling
;----------------------------------------------------------------------

  IF AIRBORNE > 0			; Display jumping player
    IF DIRECTION = LEFT
      LET IMAGE	10			; Jump left up
      LET FRAME 0
      IF CANGOLEFT
        SPRITELEFT			; Move left
      ENDIF
    ELSE
      LET IMAGE	8			; Jump right up
      LET FRAME 0
      IF CANGORIGHT
        SPRITERIGHT			; Move right
      ENDIF
    ENDIF
    IF JUMPSPEED < 127
      LET FRAME 2
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; AI handling Child/spider
;----------------------------------------------------------------------
; - look left ahead
;     seeing player spider:       20% ri,60% att,20% st
;     seeing player spider/child: 40% ri,40% att,20% st
;     seeing player child:        60% ri,20% att,20% st
;     seeing spit:                20% ri,60% att,      ,20% jmp
;
; - look right ahead
;     seeing player spider:       20% le,60% att,20% st
;     seeing player spider/child: 40% le,40% att,20% st
;     seeing player child:        60% le,20% att,20% st
;     seeing spit:                20% le,60% att       ,20% jmp
;----------------------------------------------------------------------

  GETRANDOM 5				; RND nr -> 100/5=20%

  LET J 0				; Reset collisionflag

; Checking look ahead 46 pixels

  LET I X

  IF DIRECTION LEFT			; Check left ahead
    LET K 0
    SUBTRACT 31 FROM X			; Check right ahead
  ELSE
    LET K 1
    ADD 31 TO X
  ENDIF

; Check look ahead collisions

 REPEAT 2
  IF COLLISION 0
    IF F 0
      LET J 1				; Collision Player spider
    ENDIF
    IF F 6
      LET J 2				; Collision Player Child/spider
    ENDIF
    IF F 12
      LET J 3				; Collision Player Child
    ENDIF
  ELSE
    IF COLLISION 4
      LET J 3				; Collision Child
    ENDIF
    IF COLLISION 5
      LET J 1				; Collision Spider
    ENDIF
    IF X <> I
      IF COLLISION 8
        LET J 1				; Collision Stumbled sprite
      ENDIF
    ENDIF
  ENDIF

; Checking look ahead 15 pixels

  LET X I				; Restore X
 ENDREPEAT

; Seeing Player Spider + Spider

  IF J 1				; Player Player Spider + Spider
    LET K 3				; 60% Attack
    IF RND 0				; 20% Right/Left
      IF K 0
        LET K 1
      ELSE
        LET K 0
      ENDIF
    ENDIF
    IF RND 4
      LET K 4				; 20% Stay
    ENDIF
  ENDIF

; Seeing Player Child/spider

  IF J 2				; Player Child/spider
    LET K 1				; 40% Right/Left
    IF RND 2				; 20% Attack
      LET K 3
    ENDIF
    IF RND 3				; 20% Attack
      LET K 3
    ENDIF
    IF RND 4
      LET K 4				; 20% Stay
    ENDIF
  ENDIF

; Seeing Player Child + Child

  IF J 3				; Player Child + Child
    LET K 1				; 60% Right/Left
    IF RND 3				; 20% Attack
      LET K 3
    ENDIF
    IF RND 4
      LET K 4				; 20% Stay
    ENDIF
  ENDIF

; Seeing spit

  IF COLLISION 3			; Look left spit
    LET K 3				; 60% Attack
    IF RND 0				; 20% Right/Left
      IF K 0
        LET K 1
      ELSE
        LET K 0
      ENDIF
    ENDIF
    IF RND 4
      LET K 2				; 20% Jump
    ENDIF
  ENDIF

  LET X I				; Restore X

  IF COLLISION 0			; If overlap, walk 
    LET K 0
  ENDIF

;----------------------------------------------------------------------
; Status handling in variable K:
;
; 0 - Left
; 1 - Right
; 2 - Jump
; 3 - Attack
; 4 - Stay
;----------------------------------------------------------------------


  IF SETTINGB 0				; If no delay

; Decision is ATTACK

    IF K 3				
      LET I X
      IF DIRECTION LEFT 
        SUBTRACT 16 FROM X
        LET IMAGE 9			; Spit left
        SPAWN 3 18			; Spawn spit left
        SPAWNED
          LET SETTINGB 2		; Spit speed
        ENDSPRITE
        LET K 1
      ELSE
        ADD 16 TO X
        LET IMAGE 7			; Spit right
        SPAWN 3 18			; Spawn spit right
          SPAWNED
          LET SETTINGB 2		; Spit speed
        ENDSPRITE
        LET K 0
      ENDIF 
      LET FRAME 0 
      LET X I
      LET SETTINGB 10
    ENDIF

    IF AIRBORNE 0			; If not falling

; Decision is LEFT

      IF K 0				
        LET IMAGE 10
        IF CANGOLEFT
          LET DIRECTION LEFT
          SPRITELEFT			; Continue walking 
        ELSE
          IF AIRBORNE = 0		; Check if jumping
            LET SETTINGB 20		; If not, set delay timer going left
          ELSE
            LET DIRECTION RIGHT		; In air so change direction
          ENDIF
        ENDIF
      ENDIF

; Decision is RIGHT

      IF K 1				
        LET IMAGE 8
        IF CANGORIGHT
          LET DIRECTION RIGHT
          SPRITERIGHT
        ELSE
          IF AIRBORNE = 0		; Check if jumping
            LET SETTINGB 30		; If not, set delay timer going right
          ELSE
            LET DIRECTION LEFT		; In air so change direction
          ENDIF
        ENDIF
      ENDIF

    ENDIF

; Decision is JUMP

    IF K 2				
      IF CANGOUP			; Check if can go up
        LET SETTINGB 10			; Yes, set delay jumping
        LET IMAGE 10			; Change image to jump left
        LET FRAME 0			; Reset framenr
      ENDIF
    ENDIF

; Decision is STAY

    IF K 4				
      LET SETTINGB 30			; Yes, set delay jumping
      LET IMAGE 6 			; Stand
      LET FRAME 0
    ENDIF

  ENDIF

;----------------------------------------------------------------------

  IF AIRBORNE 0
    ANIMATE FAST
  ENDIF

  FALL

;----------------------------------------------------------------------
; SPIT
;----------------------------------------------------------------------
; SETTINGA = Shoot distance counter
; SETTINGB = Shoot speed
;----------------------------------------------------------------------

EVENT SPRITETYPE3

;----------------------------------------------------------------------
; If spit, determine distance
;----------------------------------------------------------------------

  IF SETTINGA > 0			; Check spitting dstance
    SUBTRACT 1 FROM SETTINGA
    IF SETTINGA <= 0			; Remove skit/dagger after x moves
      REMOVE
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; If hit player
;----------------------------------------------------------------------

  IF H = 0				; If player not invulnerable
    IF COLLISION 0			; If collision player
      REMOVE				; Remove spit
      LET W Q				; Energy -1
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; If hit worm
;----------------------------------------------------------------------

  IF COLLISION 1			; If hit by weapon
    LET I DIRECTION
    OTHER
      REPEAT 4				; Move enemy 8 pixels 
        IF I LEFT
          IF CANGOLEFT
            SPRITELEFT
          ENDIF
        ELSE
          IF CANGORIGHT
            SPRITERIGHT
          ENDIF
        ENDIF
        FALL
      ENDREPEAT
    ENDSPRITE
    REMOVE				; Remove spit
  ENDIF

;----------------------------------------------------------------------
; If hit Spider, Spiderchild, Child
;----------------------------------------------------------------------

  LET J 5				; Spider
  LET K 19				; Image
  LET L 1				; Spritetype

  REPEAT 3

    IF COLLISION J			; If hit 
      LET I DIRECTION
      OTHER
        SUBTRACT 1 FROM SETTINGA	; Decrement hitcounter
        IF SETTINGA <= 0		; If strength = 0
          REMOVE			; Remove enemy
          SPAWN L,K			; Devolve
          LET Y 200			; Move offscreen to avoid hit

          IF J 5			; If Spider
            IF G > 0			; Decrement enemy nr if in combat room
              SUBTRACT 1 FROM G
            ENDIF
          ENDIF

        ELSE
          REPEAT 4			; Move enemy 8 pixels 
            IF I LEFT
              IF CANGOLEFT
                SPRITELEFT
              ENDIF
            ELSE
              IF CANGORIGHT
                SPRITERIGHT
              ENDIF
            ENDIF
            FALL
          ENDREPEAT
        ENDIF
      ENDSPRITE
      REMOVE				; Remove spit
      EXIT
    ENDIF

    IF J 2
      LET J 4				; Child
      LET K 6				; Image
      LET L 2				; Spritetype
    ENDIF
    IF J 5
      LET J 2				; Spiderchild
      LET K 0				; Image
      LET L 5				; Spritetype
    ENDIF

  ENDREPEAT

;----------------------------------------------------------------------
; If hit Stumbled sprite
;----------------------------------------------------------------------

  IF COLLISION 8		; If hit by weapon
    OTHER
      LET I IMAGE
    ENDSPRITE
    IF I <> 25
      OTHER
        SUBTRACT 1 FROM SETTINGA	; Decrement hitcounter
        IF SETTINGA <= 0
          REMOVE
          IF IMAGE 5		; If spider
            SPAWN 1,19		; Spawn worm
            LET Y 200

            IF G > 0		; Decrement enemy nr if in combat room
              SUBTRACT 1 FROM G
            ENDIF

          ELSE
            IF IMAGE 11
              SPAWN 5,0		; Spawn spider
            ELSE
              SPAWN 2,6		; Spawn child/spider
            ENDIF
          ENDIF
        ENDIF
      ENDSPRITE
      REMOVE
      EXIT
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; Check movements
;----------------------------------------------------------------------

  IF DIRECTION LEFT			; Check left
    IF CANGOLEFT
      REPEAT SETTINGB
        SPRITELEFT			; Move at double enemy/player speed
      ENDREPEAT
    ELSE
      REMOVE
    ENDIF
  ELSE
    IF CANGORIGHT			; Check right
      REPEAT SETTINGB
        SPRITERIGHT
      ENDREPEAT
    ELSE
      REMOVE
    ENDIF
  ENDIF

  ANIMATE SLOW

