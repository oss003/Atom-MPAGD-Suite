EVENT MAINLOOP2

;-----------------------------------------------------------------
; Energy bar:
;
; Energy 00-08 Spider
;        09-17 Childspider
;        18-26 Child
;
; Update energybar if flag set
;
; W =    0, do nothing
; W <> 128, add W to energy + redraw energybar
; W =  128, only redraw energybar
;-----------------------------------------------------------------

;-----------------------------------------------------------------
; Update energy value
;-----------------------------------------------------------------

  IF W > 0
      
    IF W <> 128
;      ADD W TO E		; Update energy

      IF E > 251		; Determine min
       LET E 0
      ENDIF

      IF E > 27			; Determine max
        LET E 27
      ENDIF
     
    ENDIF

;-----------------------------------------------------------------
; Check max energy per part of map
; Every collected object increases max with 3
; Start:         3
; Lower level :  6, 9,12
; Middle level: 15,18,21
; Upper level : 24,27
;-----------------------------------------------------------------

    LET I 27

; Upper part of map

    IF A < 37
      LET I 24
      ENDIF
    IF A < 32
      LET I 21
    ENDIF

; Middle part of map

    IF A < 26
      LET I 18
    ENDIF
    IF A < 23
      LET I 15
    ENDIF
    IF A < 18
      LET I 12
    ENDIF

; Lower part of map

    IF A < 12
      LET I 9
    ENDIF
    IF A < 7
      LET I 6
    ENDIF
    IF A < 2
      LET I 3
    ENDIF

; Adjust max energy and redraw energybar

    IF E > I
      LET E I
;      LET W 128
    ENDIF

;-----------------------------------------------------------------
; Update energy bar
;-----------------------------------------------------------------

    LET LINE 23
    LET COLUMN 3
    IF E > 0
      REPEAT E
        PUTBLOCK 34		; Draw full energyblocks
      ENDREPEAT
    ENDIF

    ADD 3 TO I
    WHILE COLUMN < I		; Dots until max
      PUTBLOCK 11
    ENDWHILE

    WHILE COLUMN < 29		; Empty until end of bar
      PUTBLOCK 27
    ENDWHILE

    LET W 0			; Reset energybar update flag

;-----------------------------------------------------------------
; Update phase
;-----------------------------------------------------------------

    LET I F			; Save current fase
    IF E > 18			; If phase child
      LET F 12
    ELSE
      IF E > 9			; If phase childspider
        LET F 6
      ELSE
        LET F 0			; If phase spider
      ENDIF
    ENDIF

; Check fase switch

    IF F <> I			
      LET H 50			; Phase switch so player temp invulnerable
    ENDIF

;-----------------------------------------------------------------
; Update jump strength according to phase
;-----------------------------------------------------------------

    IF F 0			; Player is spider
      LET V 7			; Jump strength
    ENDIF

    IF F 6			; Player is childspider
      LET V 8			; Jump strength
    ENDIF

    IF F 12			; Player is child
      LET V 8			; Jump strength
    ENDIF

  ENDIF

