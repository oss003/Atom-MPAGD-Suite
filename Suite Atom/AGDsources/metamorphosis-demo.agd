;----------------------------------------------------------------------
; PLAYER
;----------------------------------------------------------------------
; SETTINGA = Delay counter
; SETTINGB = Eat counter
; E = Energy
; H = Invulnerable, 128 = stumbled
; N = Shoot delay
;----------------------------------------------------------------------

EVENT PLAYER

;----------------------------------------------------------------------
; Endscreen
;----------------------------------------------------------------------

  IF SCREEN 6
    IF H 1
      REMOVE				; Blink player
    ENDIF
  ENDIF

;----------------------------------------------------------------------
; If energy = 0 goto demo mode
;----------------------------------------------------------------------

  IF E 0				; Check if energy gone
    IF Q > 0				; If not dead
      SPAWN 1,19			; Spawn worm
      LET Y 200				; Relocate player
      REMOVE				; Remove player
      COLOUR 130			; Blink text red/black
      AT 10,6
      PRINT " PRESS R TO RESTART "
      LET Q 0				; Set Restart flag
      LET Z 4
    ENDIF
    EXIT
  ENDIF

;----------------------------------------------------------------------
; Take object to leave combatrooms
;----------------------------------------------------------------------

  DETECTOBJ				; Check if hit object

  IF OBJ <> 255				; If hit object
    BEEP 150
    DELAY 10
    GET OBJ				; Collect object

    LET I 0

; Lower part of map

    IF OBJ 0				; If got OBJ0 (room 19)
      LET A 1
      LET I 1
    ENDIF
    IF OBJ 1				; If got OBJ1 (combatroom 18)
      LET A 6
      LET I 1
    ENDIF
    IF OBJ 2				; If got OBJ2 (combatroom 20)
      LET A 11
      LET I 1
    ENDIF

; Middle part of map

    IF OBJ 3				; If got OBJ3 (combatroom 13)
      LET A 17
      LET I 1
    ENDIF
    IF OBJ 4				; If got OBJ4 (combatroom 24)
      LET A 22 
      LET I 1
    ENDIF
    IF OBJ 5				; If got OBJ5 (room 15)
      LET A 25
      LET I 1
    ENDIF

; Upper part of map

    IF OBJ 6				; If got OBJ6 (combatroom 27)
      LET A 31 
      LET I 1
    ENDIF
    IF OBJ 7				; If got OBJ7 (combatroom 8)
      LET A 36
      LET I 1
    ENDIF
    IF OBJ 8				; If got OBJ8 (room 17)
      LET A 39
      LET I 1
    ENDIF

; Final combatroom

    IF OBJ 9				; If got OBJ9 (combatroom 24)
      LET A 43 
      LET I 1
      LET D 6
      LET H 50
    ENDIF

; Combatroom batch

    AT 23,1
    IF I 0
      IF OBJ 10				; If got combatroom batch
        ADD 1 TO A
        PUTBLOCK 28			; Show batch in possesion
      ENDIF
    ELSE
      PUTBLOCK 30			; Show object in possesion
    ENDIF

;----------------------------------------------------------------------
; Check if leaving combat room
;----------------------------------------------------------------------

    LET I 0

; Lower part of map

    IF SCREEN 18			; Combatroom 1
      LET I 1
    ENDIF
    IF SCREEN 20			; Combatroom 2
      LET I 1
    ENDIF

; Middle part of map

    IF SCREEN 13			; Combatroom 3
      LET I 1
    ENDIF
    IF SCREEN 28			; Combatroom 4
      LET I 1
    ENDIF

; Upper part of map

    IF SCREEN 27			; Combatroom 5
      LET I 1
    ENDIF
    IF SCREEN 8 			; Combatroom 6
      LET I 1
    ENDIF

; Final combatroom

    IF SCREEN 24			; Combatroom 7
      LET I 1
    ENDIF

; Leave combatroom

    IF I 1
      LET I 40
      REPEAT 20				; Play sound
        BEEP I
        ADD 2 TO I
        DELAY 1
      ENDREPEAT
      DELAY 20
      COLOUR 4
      CLW
      PRINTMODE 1			; Print text
      AT 11,4
      PRINT "COMBAT ROOM COMPLETED!!!"
      DELAY 100
      PRINTMODE 0
      LET SCREEN D			; Save return screen
      RESTART
      LET D 0				; Reset return screen

      AT 23,1				; Show object in possesion
      PUTBLOCK 30
      LET Z 2
    ENDIF

  ENDIF

