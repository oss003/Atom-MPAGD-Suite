;----------------------------------------------------------------------
; TOXIC DROP
;----------------------------------------------------------------------

EVENT SPRITETYPE6

 IF IMAGE 24
  ANIMATE SLOW

;----------------------------------------------------------------------
; Check collisions
;----------------------------------------------------------------------

  IF H 0				; If vulnarable
    IF COLLISION 0			; If collision player
      CRASH 50
      REMOVE
      LET W Q				; Energy -1
      EXIT
    ENDIF
  ENDIF

  LET I 2				; If collision spider, childspider, child
  LET J 0
  LET K 5

  REPEAT 3
    IF COLLISION I
      OTHER
        SUBTRACT 1 FROM SETTINGA	; Decrement strength
        IF SETTINGA <= 0		; If strength = 0
          REMOVE			; Remove enemy
          SPAWN K,J			; Devoluate
          LET Y 200			; Move offscreen to avoid hit

          IF I 5			; If Spider
            IF G > 0			; Decrement enemy nr if in combat room
              SUBTRACT 1 FROM G
            ENDIF
          ENDIF
        ENDIF
      ENDSPRITE
      REMOVE				; Remove toxic drop
      LET Y 200
    ENDIF

    IF I 4
      LET I 5				; Spider
      LET J 19
      LET K 1
    ENDIF
    IF I 2
      LET I 4				; Spiderchild
      LET J 6
      LET K 2
    ENDIF

  ENDREPEAT

;----------------------------------------------------------------------
; Movements
;----------------------------------------------------------------------

  IF CANGODOWN
    SPRITEDOWN
    SPRITEDOWN
  ELSE
    IF SCREEN 24
      IF G 3
        SPAWN 1,19			; Spawn worm
      ENDIF
      IF G < 3
        SPAWN 4,12			; Spawn child
        ADD 1 TO G
      ENDIF
    ENDIF
    REMOVE				; Remove toxic drop
  ENDIF

 ENDIF
;----------------------------------------------------------------------
; DRIPPING ROCK
;----------------------------------------------------------------------
; SETTINGA = 
; SETTINGB = Delay timer
;----------------------------------------------------------------------

EVENT SPRITETYPE7

  ANIMATE SLOW

  IF SETTINGB = 0			; If end of delay
    SPAWN 6,24				; Spawn toxic drop
    GETRANDOM 2
    ADD 2 TO RND
    MULTIPLY RND BY 25
    LET SETTINGB RND			; Set delaytime 2-3 sec
  ENDIF

  SUBTRACT 1 FROM SETTINGB		; Decrement delay

;----------------------------------------------------------------------
; STUMBLED ENEMY OR TELEPORT
;----------------------------------------------------------------------
;
; If enemy hit Mario-style, the spritetype changes into stumbled type 8
; Strength is copied back if stumble phase is over
;----------------------------------------------------------------------
; STUMBLED ENEMY
;   SETTINGA = Strength
;   SETTINGB = Delay counter
;
; TELEPORT
;   SETTINGA = combatroom/exit level
;   SETTINGB = Timer expire
;----------------------------------------------------------------------

EVENT SPRITETYPE8

;----------------------------------------------------------------------
; If sprite is teleport
;----------------------------------------------------------------------

  IF IMAGE 25				; If sprite is teleport

; Check spawning/removing teleport in combatroom 24

    IF SCREEN 24

      IF SETTINGB B
        IF U 100			; Pink teleport expire
        ELSE
          IF U 200			; Pink teleport expire
          ELSE
            SPAWN 6,24			; Spawn toxic drop
          ENDIF
        ENDIF
        LET SETTINGB 0
      ENDIF

      IF B U				; If timer expired
        LET U 0				; Reset teleportflag
        REMOVE				; Remove teleport
      ENDIF

    ENDIF

; If object taken, remove teleport

; Lower part of map

    IF SCREEN 18
      IF S 1
        REMOVE
      ENDIF
    ENDIF
    IF SCREEN 20
      IF S 1
        REMOVE
      ENDIF
    ENDIF

; Middle part of map

    IF SCREEN 13
      IF S 1
        REMOVE
      ENDIF
    ENDIF
    IF SCREEN 28
      IF S 1
        REMOVE
      ENDIF
    ENDIF

; Upper part of map

    IF SCREEN 27
      IF S 1
        REMOVE
      ENDIF
    ENDIF
    IF SCREEN 8
      IF S 1
        REMOVE
      ENDIF
    ENDIF

; Final combatroom

    IF SCREEN 24
      IF S 1
        REMOVE
      ENDIF
    ENDIF

    ANIMATE SLOW			; Animate

;----------------------------------------------------------------------
; Check if entering combat room
;----------------------------------------------------------------------

    IF COLLISION 0			; If player hits teleport

      IF SETTINGA 0			; Combatroom teleports

        LET J 0

; Lower part of map

        IF SCREEN 15
          LET J 18
        ENDIF
        IF SCREEN 17
          LET J 20
        ENDIF

; Middle part of map

        IF SCREEN 14
          LET J 13
        ENDIF
        IF SCREEN 12
          LET J 28
        ENDIF

; Upper part of map

        IF SCREEN 7
          LET J 27
        ENDIF
        IF SCREEN 5
          LET J 8
        ENDIF

; Final combatroom

        IF SCREEN 9
          LET J 24
        ENDIF

; Enter combatroom

        IF J > 0
          LET I 80
          REPEAT 20
            BEEP I
            SUBTRACT 2 FROM I
            DELAY 1
          ENDREPEAT
          LET D SCREEN			; Save return screen
          LET SCREEN J
          RESTART
          EXIT
        ENDIF

      ENDIF

;----------------------------------------------------------------------
; Check if leaving combat room
;----------------------------------------------------------------------

      LET I 0

; Lower part of map

     IF SCREEN 18
        LET I 1
      ENDIF
      IF SCREEN 20
        LET I 1
      ENDIF

; Middle part of map

      IF SCREEN 13
        LET I 1
      ENDIF
      IF SCREEN 28
        LET I 1
      ENDIF

; Upper part of map

      IF SCREEN 27
        LET I 1
      ENDIF
      IF SCREEN 8
        LET I 1
      ENDIF

; Final comatroom

      IF SCREEN 24
        LET I 1
      ENDIF

;----------------------------------------------------------------------
; Leave combatroom/exit to next part of the map
;----------------------------------------------------------------------

      IF I 1

; Leave combatroom

        LET I 40
        REPEAT 20
          BEEP I
          ADD 2 TO I
          DELAY 1
        ENDREPEAT
        DELAY 20
        LET SCREEN D			; Save return screen
        RESTART
        LET D 0
        LET Z 2
      ELSE

; Exit to next part of the map

        COLOUR 2
        CLW
        DELAY 50			; Wait 2 sec

        LET I SCREEN			; Save screen
 
        LET J 0
        IF A 12				; Only display head once
          LET J 1
        ENDIF
        IF A 26				; Only display head once
          LET J 1
        ENDIF
        IF A 40				; Only display head once
          LET J 1
        ENDIF

        IF J 1
          LET SCREEN 6			; Screen = head
          USER 1			; Show screen
          DELAY 50			; Wait 2 sec
          USER 0			; Fade

          CLW
          COLOUR 2
          AT 11,2			; Print text
          PRINTMODE 1
          PRINT "SOON YOU WILL BE ONE OF US!!"
          PRINTMODE 0
          DELAY 75			; Wait 3 sec
          USER 0			; Fade

          ENDGAME
          EXIT

        ENDIF

        LET SCREEN I			; Reset screen

        IF SCREEN 9			; Teleport upper part->head
          LET D 9
          LET SCREEN 24
          OTHER
            LET X 120
            LET Y 96
          ENDSPRITE
          IF A 40
            LET A 41
          ENDIF
        ENDIF

        IF SCREEN 11			; Teleport middle->upper part
          LET SCREEN 9
          OTHER
            LET X 120
            LET Y 136
          ENDSPRITE
          IF A 26
            LET A 27
          ENDIF
        ENDIF

        IF SCREEN 19			; Teleport lower->middle part
          LET SCREEN 16
          OTHER
            LET X 120
            LET Y 72
          ENDSPRITE
          IF A 12
            LET A 13
          ENDIF
        ENDIF

        IF SCREEN <> 24			; If not last combatroom
          USER 1			; Show screen
        ENDIF

      ENDIF

    ENDIF
    EXIT				; Exit routine

  ENDIF

;----------------------------------------------------------------------
; Sprite is stumbled enemy
;----------------------------------------------------------------------

  IF SETTINGB > 0
    SUBTRACT 1 FROM SETTINGB		; Decrement delay counter
  ENDIF

  LET I 0
  IF SETTINGB 0				; Enemy timer expired
    LET I 1
  ENDIF
  IF SETTINGB 128			; Player timer expired
    LET I 1
  ENDIF

  IF I 1

    REMOVE				; Remove stumble and spawn new

    LET I SETTINGA

    LET J 5				; Spider
    LET K 0				; Spider standing
    LET M 5				; Spritetype

    REPEAT 3
      IF IMAGE J
        IF SETTINGB 128			; If player
          SPAWN 0,K
        ELSE
          SPAWN M,K			; Spider
        ENDIF
        SPAWNED
          LET SETTINGA I		; Copy strength
          LET SETTINGB 0
        ENDSPRITE
        EXIT
      ENDIF

      IF J 17
        LET J 11
        LET K 6				; Spider standing
        LET M 2				; Spritetype
      ENDIF
      IF J 5
        LET J 17
        LET K 12			; Spider standing
        LET M 4				; Spritetype
      ENDIF

    ENDREPEAT

  ENDIF
