      SUBTRACT 2 FROM COLUMN
      PUTBLOCK 162
      PUTBLOCK 163
      SUBTRACT 1 FROM LINE
      DELAY 1
    ENDREPEAT

    SUBTRACT 2 FROM COLUMN		; Top to bottom
    REPEAT K
      PUTBLOCK 140
      PUTBLOCK 141
      ADD 1 TO LINE
      SUBTRACT 2 FROM COLUMN
      PUTBLOCK 162
      PUTBLOCK 163
      ADD 1 TO LINE
      SUBTRACT 2 FROM COLUMN
      DELAY 1
    ENDREPEAT

    SUBTRACT 2 FROM LINE		; Right to left
    REPEAT J
      PUTBLOCK 140
      PUTBLOCK 141
      ADD 1 TO LINE
      SUBTRACT 2 FROM COLUMN
      PUTBLOCK 162
      PUTBLOCK 163
      SUBTRACT 1 FROM LINE
      SUBTRACT 4 FROM COLUMN
      DELAY 1
    ENDREPEAT

    ADD 2 TO COLUMN			; Bottom to top
    REPEAT K
      PUTBLOCK 140
      PUTBLOCK 141
      ADD 1 TO LINE
      SUBTRACT 2 FROM COLUMN
      PUTBLOCK 162
      PUTBLOCK 163
      ADD 1 TO LINE
      SUBTRACT 2 FROM COLUMN
      SUBTRACT 4 FROM LINE
      DELAY 1
    ENDREPEAT

    ADD 2 TO I				; Adjust start x and y
    SUBTRACT 2 FROM J			; Adjust wide
    SUBTRACT 2 FROM K			; Adjust height

  ENDREPEAT

  DELAY 50

  COLOUR 0				; Reset colour


