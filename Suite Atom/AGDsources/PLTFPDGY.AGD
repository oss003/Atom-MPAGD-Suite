;IF X <=  8 
;  SCREENLEFT 
;  LET X =  228 
;  SUBTRACT  1 FROM Y 
;ENDIF 
IF X > =  230 
  SCREENRIGHT 
  SCORE 50
 	AT 23 17
	SHOWSCORE
  LET X =  10 
  SUBTRACT  1 FROM Y 
ENDIF 
DETECTOBJ 
IF OBJ =  255 
ELSE 
  IF OBJ =  0 
    GET OBJ 
    SCORE  100 
	 AT 23 17
	 SHOWSCORE
    LET LINE =  4 
    LET COLUMN =  0 
    MESSAGE  0 
    DELAY 40
    WAITKEY 
    REDRAW 
	AT 23 10
	PRINT"Score: "
	AT 23 17
	SHOWSCORE
	AT 23 24
	PRINT"Lives: "
	AT 23 30
	DISPLAY LIVES 
  ENDIF 
  IF OBJ =  13 
    GET OBJ 
    SCORE  100 
	 AT 23 17
    SHOWSCORE 
    ENDGAME 
  ELSE 
    GET OBJ 
    SCORE  100 
;    LET LINE =  22 
;    LET COLUMN =  24 
	 AT 23 17
    SHOWSCORE 
    BEEP  100 
  ENDIF 
ENDIF 
IF KEY RIGHT 
  IF CANGORIGHT 
    LET IMAGE =  0 
    ANIMATE 
    SPRITERIGHT 
  ENDIF 
ENDIF 
IF KEY LEFT 
  IF CANGOLEFT 
    LET IMAGE =  1 
    ANIMATE 
    SPRITELEFT 
  ENDIF 
ENDIF 
IF KEY UP
  IF LADDERABOVE
    SPRITEUP
  ENDIF
ENDIF 
IF KEY DOWN
  IF LADDERBELOW
    SPRITEDOWN
  ENDIF
ENDIF 
IF KEY FIRE 
	IF LADDERABOVE
		SPRITEUP
	ELSE
		TABLEJUMP 
	ENDIF
ENDIF 
IF DEADLY 
  KILL 
ENDIF 

EVENT SPRITETYPE1 
IF SETTINGA =  0 
  IF CANGOUP 
    SPRITEUP 
  ELSE 
    LET SETTINGA =  1 
  ENDIF 
ELSE 
  IF CANGODOWN 
    SPRITEDOWN 
  ELSE 
    LET SETTINGA =  0 
  ENDIF 
ENDIF 
ANIMATE 
IF COLLISION  0 
  KILL 
ENDIF 

EVENT SPRITETYPE2 
IF SETTINGA =  0 
  IF CANGOLEFT 
    SPRITELEFT 
    SUBTRACT  16 FROM X 
    IF CANGODOWN 
      LET SETTINGA =  1 
    ENDIF 
    ADD  16 TO X 
  ELSE 
    LET SETTINGA =  1 
  ENDIF 
ELSE 
  IF CANGORIGHT 
    SPRITERIGHT 
    ADD  16 TO X 
    IF CANGODOWN 
      LET SETTINGA =  0 
    ENDIF 
    SUBTRACT  16 FROM X 
  ELSE 
    LET SETTINGA =  0 
  ENDIF 
ENDIF 
ANIMATE 
IF COLLISION  0 
  KILL 
ENDIF 

EVENT SPRITETYPE3 
IF SETTINGA =  0 
