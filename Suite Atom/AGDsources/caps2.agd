EVENT PLAYER
DETECTOBJ
IF OBJ = 255
ELSE
    IF OBJ > 111
        ADD 1 TO LIVES
        BEEP 60
    ENDIF
    IF OBJ <= 10
        SCORE 200
        ADD 1 TO C
        ADD 10 TO K
        BEEP 75
        ; COLOUR 112
        LET LINE = 22
        LET COLUMN = 16
        SHOWSCORE
        ; COLOUR 70
        LET LINE = 21
        LET COLUMN = 29
        DISPLAY C
    ENDIF
    IF OBJ = 111
        ADD 1 TO D
        BEEP 60
        ENDGAME
    ELSE
        GET OBJ
        SCORE 100
        ADD 1 TO D
        BEEP 50
        ; COLOUR 112
        LET LINE = 22
        LET COLUMN = 16
        SHOWSCORE
    ENDIF
    ; COLOUR 70
    LET LINE = 21
    LET COLUMN = 7
    DISPLAY LIVES
ENDIF
IF X > 127
    LET M = 1
ELSE
    LET M = 0
ENDIF
IF KEY RIGHT
    LET DIRECTION = 1
    IF X > 231
        SCREENRIGHT
        LET X = 20
        EXIT
    ELSE
        IF CANGORIGHT
            LET IMAGE = 1
            IF A = 0
                ANIMATE
            ENDIF
            SPRITERIGHT
        ENDIF
    ENDIF
ENDIF
IF KEY LEFT
    LET DIRECTION = 0
    IF X <= 12
        SCREENLEFT
        LET X = 219
        EXIT
    ELSE
        IF CANGOLEFT
            LET IMAGE = 0
            IF A = 0
                ANIMATE
            ENDIF
            SPRITELEFT
        ENDIF
    ENDIF
ENDIF
IF KEY UP
    TABLEJUMP
ENDIF
IF KEY FIRE
    TABLEJUMP
ENDIF
