EVENT PLAYER

IF SCREEN > 0
	; Reset 'is flying' setting
	LET SETTINGA = 0
	; Reset 'is running l/r' setting
	LET SETTINGB = 0

	; Update 'Bored' counter
	IF B < 255
		ADD 1 TO B
	ENDIF
	
	; Immunity counter
	IF J > 0
		SUBTRACT 1 FROM J
	ENDIF
	
	; Player died stage 3
	IF K = 3
		IF Y < Q
			LET SETTINGA = 1
			SPRITEDOWN
			ANIMATE
			USER 5
			EXIT
		ELSE
			LET K = 0
			LET B = 0 ; Reset 'bored' counter
			INK 1
			SUBTRACT 1 FROM LIVES
			AT 0,24
			DISPLAY LIVES
			INK 3
			LET J = 20 ; Immunity counter
		ENDIF
	ENDIF
	IF K = 0
		IF KEY UP
			LET B = 0
			USER 5
			LET SETTINGA = 1
			IF Y = TOPEDGE
				; Cannot SCREENUP if screen is start of level
				LET P = 0
				LET I = 0
				WHILE P = 0
					WHILE I < 9
						READ Q
						ADD 1 TO I
						IF Q = SCREEN
							LET P = 1
						ENDIF
					ENDWHILE
					IF P = 0
						SCREENUP
						LET Y = 44
						LET D = 0
						LET P = 1
					ENDIF
				ENDWHILE
				IF P = 0
					EXIT
				ENDIF
			ENDIF
			IF CANGOUP
				SPRITEUP
				SPRITEUP
			ENDIF
		ELSE
			IF Y > 46
				IF SCREEN = 16
					ADD 1 TO SCREEN
				ELSE
					SCREENDOWN
				ENDIF
				LET Y = TOPEDGE
				LET D = 0
				EXIT
			ENDIF
			IF CANGODOWN
				LET B = 0
				LET SETTINGA = 1
				USER 5
				ADD 2 TO Y
				IF DEADLY
					LET K = 1
					SUBTRACT 6 FROM Y
					EXIT
				ENDIF
			ENDIF
		ENDIF

		IF KEY LEFT
			LET DIRECTION = LEFT
			SUBTRACT 2 FROM X
			LET B = 0
			IF CUSTOM
				ADD 2 TO X
			ELSE
				ADD 2 TO X
				IF X <= 4
					SCREENLEFT
					LET X = RIGHTEDGE
					EXIT
				ELSE
					IF CANGOLEFT
						LET SETTINGB = 1
						SPRITELEFT
						SPRITELEFT
					ENDIF
				ENDIF
			ENDIF
		ENDIF

		IF KEY RIGHT
			LET DIRECTION = RIGHT
			ADD 2 TO X
			LET B = 0
			IF CUSTOM
				SUBTRACT 2 FROM X
			ELSE
				SUBTRACT 2 FROM X
				IF X >= RIGHTEDGE
					SCREENRIGHT
					LET X = LEFTEDGE
					EXIT
				ELSE
					IF CANGORIGHT
						LET SETTINGB = 1
						SPRITERIGHT
						SPRITERIGHT
					ENDIF
				ENDIF
			ENDIF
		ENDIF

		; Sprite animation
		IF B < 100
			IF DIRECTION = LEFT
				IF SETTINGA = 0
					LET IMAGE = 3
				ELSE
					LET IMAGE = 1
				ENDIF
			ELSE
				IF SETTINGA = 0
					LET IMAGE = 2
				ELSE
					LET IMAGE = 0
				ENDIF
			ENDIF
		ELSE
			; HERO is bored!
			LET IMAGE = 13
		ENDIF
		
		; Animate normal or bored speed.
		IF IMAGE <> 13
			ANIMATE
		ELSE
			ANIMATE MEDIUM
		ENDIF
		
		; Stand still
		IF SETTINGB = 0
		  IF SETTINGA = 0
			IF B < 100
				LET FRAME = 0
			ENDIF
		  ENDIF
		ENDIF

		; Shoot laser

		IF Z > 0
			SUBTRACT 1 FROM Z
		ENDIF

		IF KEY FIRE
			IF Z 0
				LET Q = 1
				USER 2
				LASER DIRECTION
				LET Z 3
			ENDIF
		ELSE
			LET F = 0
		ENDIF
		
		; Drop dynamite
		IF KEY FIRE2
			IF AIRBORNE = 0
				; Do they have dynamite remaining?
				IF E > 0
					IF D = 0
						SUBTRACT 1 FROM E
						USER 7
						SPAWN 3 6
						IF DIRECTION = RIGHT
							SPAWNED
								ADD 4 TO X
							ENDSPRITE
							ELSE
							SPAWNED
								SUBTRACT 4 FROM X
							ENDSPRITE
						ENDIF
						LET D = 20
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	
	IF DEADLY
		IF K = 0
			IF DIRECTION = RIGHT
				SUBTRACT 4 FROM X
			ELSE
				ADD 4 TO X
			ENDIF
			LET K = 1
		ENDIF
	ENDIF

	; Rescued miner
	IF R = 1
		; Countdown dynamites left
		INK 1
		WHILE E > 0
			SUBTRACT 1 FROM E
			CRASH 5
			SCORE 20
			AT 0,5
			SHOWSCORE
			AT 0,29
			DISPLAY E
		ENDWHILE
		; Countdown time left
		WHILE T > 0
			SUBTRACT 1 FROM T
			AT 0,16
			DISPLAY SECONDS T
			SCORE 1
		    AT 0,5
		    SHOWSCORE
			LET Q = 151
			SUBTRACT T FROM Q
			USER 3
		ENDWHILE
		; Add extra lives
		IF SCREEN = 12
			ADD 1 TO LIVES
		ENDIF
		IF SCREEN = 38
			ADD 1 TO LIVES
		ENDIF
		LET T = 100; Seconds to complete level
		LET S = 30;  Counter for seconds
		LET D = 0;   No dynamite in use
		LET E = 6;	 Dynamite left
		LET R = 0;	 No miners rescued yet.
		LET X = 48;
		LET Y = 8;
		LET DIRECTION = RIGHT;
		; Draw startscreen for new cave
		INK 3
		ADD 1 TO SCREEN
		USER 9
		RESTART
		EXIT
	ENDIF

	; Touched miner
	IF COLLISION 1
		OTHER
			LET FRAME = 1
		ENDSPRITE
		SCORE 100
		LET R = 1;
	ENDIF

	; Player died stage 2
	IF K = 2
		; Player dead sfx
		LET Q = 3
		USER 2
		; Pause
		LET K = 12
		WHILE K > 0
			SUBTRACT 1 FROM K
			DELAY 7
		ENDWHILE
		LET Q = Y;
		LET Y = TOPEDGE;
		LET K = 3;
		IF DIRECTION = RIGHT
			LET IMAGE = 0
		ELSE
			LET IMAGE = 1
		ENDIF
		IF LIVES = 1
			LET G = 1
			DELAY 100
			AT 3,13
			PRINT "PRESS A KEY!"
			WAITKEY
		ENDIF
	ENDIF
	
	; Player died
	IF K = 1
		IF J = 0
			; Change image to player dead sprite
			LET FRAME = 0
			LET IMAGE = 8
			LET K = 2
			IF LIVES = 1
				INK 2
				AT 2,11
				PRINT "-  GAME OVER!  -"
				LET F = 1
				LET Q = 2
			ENDIF
		ELSE
			LET K = 0
		ENDIF
	ENDIF
	
	; Collided with snake
	IF COLLISION 5
		; Different for snake
		OTHER
			LET P = FRAME
		ENDSPRITE
		; Only die on certain snake frames
		IF P > 2
			IF P < 4
				; Player bitten
				LET K = 1
				OTHER
					REMOVE
				ENDSPRITE
			ENDIF
		ENDIF
	ENDIF
	
	; Collided with other enemies
	IF COLLISION 2
		LET K = 1
		OTHER
			REMOVE
		ENDSPRITE
	ENDIF
	
	; End of game
	IF SCREEN = 75
	  IF G = 0
		INK 3
		AT 3,8
		PRINT "CONGRATULATIONS!"
		INK 1
		AT 4,2
		PRINT "YOU SAVED ALL OF THE MINERS!"
		AT 5,5
		PRINT "YOU REALLY ARE A HERO!"
		LET G = 255
	  ENDIF
	ENDIF
	
	; Countdown concluded game
	IF G > 1
		SUBTRACT 1 FROM G
	ENDIF
	IF G = 1
		LET F = 1
		LET Q = 2
		ENDGAME
	ENDIF	

ELSE
	IF F = 1
		IF Q > 0
			SUBTRACT 1 FROM Q
		ELSE
			; Draw Titlescreen
			USER 0
			LET Y = 1
			PAPER 0
			INK 2
			AT 4,2
			PRINT "Copyright 2022 Jason Oakley"
			INK 3
			AT 5,4
			PRINT "Press -FIRE- to start!"
			INK 2
			AT 7,0
			TICKER 32 "  Keys: Left/Right: N/M. Q to fly up. A to drop dynamite. Space to shoot. Or Joystick: Fire to shoot. Arm to drop dynamite. Thanks to Kees again for all his help. Visit BlueBilby.com ...    "
			PAPER 0
			INK 1
			AT 6,6
			PRINT "High score:"
			AT 6,18
			SHOWHIGH
			AT 6,24
			PRINT "0"
			LET F = 2	
		ENDIF
	ENDIF
	IF F = 2
		ANIMATE MEDIUM
		IF KEY FIRE
			LET F = 3
			TICKER 0
			USER 4
			LET Q = 170
			USER 5
			CLS
			; Reset colour palette for game blocks
			INK 3
			SCREENDOWN
			;LET SCREEN = 21
			LET Y = TOPEDGE
			LET X = 40
			; Set up game variables
			LET LIVES = 3
			LET T = 100; Seconds to complete level
			LET S = 30;  Counter for seconds
			LET K = 0;   Player not dead
			LET E = 6;	 Dynamite left
			LET B = 0;	 Reset 'Bored' counter
			LET G = 0;   Reset Game completed counter
			LET H = 0; 	 Game over counter
		ENDIF
	ENDIF
ENDIF

