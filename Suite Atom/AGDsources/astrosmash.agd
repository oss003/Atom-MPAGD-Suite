EVENT SPRITETYPE1
;IF CUSTOM
    ; SPRITEINK 68
;ELSE
    ; SPRITEINK 71
;ENDIF
REPEAT 3
    IF CANGOUP
        SPRITEUP
    ELSE
        LET E 0
        REMOVE
        EXIT
    ENDIF
ENDREPEAT
IF COLLISION 3
    BEEP 30
    LET E 0
    REMOVE
    OTHER
    ; SPRITEINK 71
    IF C > 0
        SUBTRACT 1 FROM C
    ENDIF
    REMOVE
    GETRANDOM 5
    IF RND = 4
    ELSE
        IF RND = 3
            SPAWN 6 10
			SPAWNED
			GETRANDOM 150
			LET X RND
			ADD 50 TO X
			ENDSPRITE
        ELSE
            IF RND = 2
                SPAWN 6 11
				SPAWNED
				GETRANDOM 150
				LET X RND
				ADD 50 TO X
				ENDSPRITE
            ELSE
                IF RND <= 1
                    SPAWN 6 10
					SPAWNED
					GETRANDOM 150
					LET X RND
					ADD 50 TO X
					ENDSPRITE
                    SPAWN 6 11
					SPAWNED
					GETRANDOM 150
					LET X RND
					ADD 50 TO X
					ENDSPRITE
                ENDIF
            ENDIF
        ENDIF
    ENDIF
    ENDSPRITE
    SCORE 10
    ADD 1 TO S
ENDIF
IF COLLISION 4
    BEEP 30
    LET E 0
    REMOVE
    OTHER
    ; SPRITEINK 71
    IF C > 0
        SUBTRACT 1 FROM C
    ENDIF
    REMOVE
    ENDSPRITE
    SCORE 10
    ADD 1 TO S
ENDIF
IF COLLISION 5
    BEEP 30
    LET E 0
    REMOVE
    OTHER
    ; SPRITEINK 71
    IF C > 0
