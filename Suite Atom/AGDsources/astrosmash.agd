EVENT PLAYER
; SPRITEINK 71
IF D = 0
    SPAWN 7 17
    KILL
ENDIF
IF COLLISION 2
    SPAWN 7 17
    KILL
ELSE
    IF COLLISION 3
        SPAWN 7 17
        KILL
    ELSE
        IF COLLISION 4
            SPAWN 7 17
            KILL
        ELSE
            IF COLLISION 5
                SPAWN 7 17
                KILL
            ELSE
                IF COLLISION 6
                    SPAWN 7 17
                    KILL
                ELSE
                    IF COLLISION 8
                        SPAWN 7 17
                        KILL
                    ENDIF
                ENDIF
            ENDIF
        ENDIF
    ENDIF
ENDIF
IF M = 1
    IF B = 0
        ADD 1 TO T
    ENDIF
    IF T = 2
        SPAWN 1 1
        LET T 0
    ENDIF
ENDIF
IF KEY RIGHT
    REPEAT 3
        IF CANGORIGHT
            SPRITERIGHT
        ENDIF
    ENDREPEAT
ENDIF
IF KEY LEFT
    REPEAT 3
        IF CANGOLEFT
            SPRITELEFT
        ENDIF
    ENDREPEAT
ENDIF
IF KEY UP
    IF M = 1
    ELSE
        IF SETTINGB = 0
            GETRANDOM 85
            MULTIPLY RND BY 2
            LET X RND
            ADD 32 TO X
            LET SETTINGB 1
        ENDIF
    ENDIF
ELSE
    IF M = 1
    ELSE
        LET SETTINGB 0
    ENDIF
ENDIF
IF KEY FIRE
    IF M = 1
        IF SETTINGB = 0
            GETRANDOM 85
            MULTIPLY RND BY 2
            LET X RND
            ADD 32 TO X
            LET SETTINGB 1
        ENDIF
    ELSE
        IF E = 0
            LET E 1
            SPAWN 1 1
        ENDIF
    ENDIF
ELSE
    IF M = 1
        LET SETTINGB 0
    ENDIF
ENDIF
IF KEY FIRE2
    LET M 1
ENDIF
IF KEY FIRE3
    LET M 0
ENDIF
; SPRITEINK 71
LET N X
IF S > 124
    IF Q = 0
        IF LIVES <= 19
            ADD 1 TO LIVES
            LET Q 1
        ENDIF
    ENDIF
    IF LIVES > 9
        LET LINE 21
        LET COLUMN 23
        ; COLOUR 68
        DISPLAY LIVES
    ELSE
