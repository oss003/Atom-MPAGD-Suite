ENDIF
DETECTOBJ
IF OBJ = 255
ELSE
    IF OBJ = 0
        GET OBJ
        SCORE 100
        LET LINE = 23
        LET COLUMN = 24
        SHOWSCORE
        LET LINE = 2
        LET COLUMN = 2
        ; COLOUR 3
        MESSAGE 2
        DELAY 40
        WAITKEY
        REDRAW
    ENDIF
    IF OBJ = 14
    	  CLS
        LET LINE = 2
        LET COLUMN = 2
        ; COLOUR 3
        MESSAGE 9
        DELAY 40
        WAITKEY
        REDRAW
    ENDIF
    IF OBJ = 1
        GET OBJ
        SCORE 100
        LET LINE = 23
        LET COLUMN = 24
        SHOWSCORE
        ENDGAME
    ELSE
        GET OBJ
        SCORE 100
        LET LINE = 23
        LET COLUMN = 24
        SHOWSCORE
    ENDIF
ENDIF
IF KEY RIGHT
    TRAIL
    IF CANGORIGHT
        LET IMAGE = 1
        ANIMATE
        SPRITERIGHT
    ENDIF
ENDIF
; warning - ENDIF without IF?
