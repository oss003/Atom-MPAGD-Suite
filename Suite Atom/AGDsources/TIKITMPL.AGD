IF KEY LEFT
    TRAIL
    IF CANGOLEFT
        LET IMAGE = 0
        ANIMATE
        SPRITELEFT
    ENDIF
ENDIF
; warning - ENDIF without IF?
;ENDIF
IF KEY UP
    TRAIL
    IF LADDERABOVE
    	SPRITEUP
    ELSE
    	TABLEJUMP
    ENDIF
ENDIF
IF DEADLY
    KILL
ENDIF


