EVENT MAINLOOP1
IF T 25
    SUBTRACT 1 FROM S
    IF S 255
        ENDGAME
        EXIT
    ENDIF
    LET T 0
    LET I S
    DIVIDE I BY 4
    LET LINE 22
    LET COLUMN 16
    IF I > 0
        REPEAT I
            PUTBLOCK 3
        ENDREPEAT
    ENDIF
    LET J S
    MULTIPLY I BY 4
    SUBTRACT I FROM J
    IF J > 0
        ADD 3 TO J
        PUTBLOCK J
    ENDIF
    PUTBLOCK 0
ENDIF
ADD 1 TO T
IF L > 0
    SUBTRACT 1 FROM L
ENDIF
IF B = 1
    SPAWN 2 12
    SPAWNED
    LET X Q
    IF DIRECTION = 1
        ADD 11 TO X
        LET FRAME 0
    ELSE
        SUBTRACT 11 FROM X
        LET FRAME 4
    ENDIF
    LET Y P
    EXIT
    LET B 2
ENDIF


