EVENT SPRITETYPE5
IF A = 1
    GETRANDOM 50
ENDIF
IF A = 2
    GETRANDOM 30
ENDIF
IF A = 3
    GETRANDOM 10
ENDIF
IF RND = 5
    IF M > O
        LET LINE 0
        LET COLUMN 0
        DISPLAY A
        GETRANDOM 4
        LET I R
        IF RND = 3
            IF I > 63
                EXIT
            ENDIF
        ENDIF
        MULTIPLY I BY 4
        IF RND = 2
            IF I > 63
                EXIT
            ENDIF
        ENDIF
        MULTIPLY I BY 4
        IF RND = 1
            IF I > 63
                EXIT
            ENDIF
        ENDIF
        MULTIPLY I BY 4
        IF RND = 0
            IF I > 63
                EXIT
            ENDIF
        ENDIF
        ADD 1 TO O
        MULTIPLY RND BY 32
        ADD 40 TO RND
        LET Y RND
        GETRANDOM 2
        IF RND = 0
            LET X 16
            SPAWN 1 5
        ELSE
            LET X 216
            SPAWN 3 5
        ENDIF
        SPAWNED
        GETRANDOM 4
        MULTIPLY RND BY 2
        ADD 2 TO RND
        IF X 16
            ADD 1 TO RND
        ENDIF
        LET IMAGE RND
    ENDIF
ENDIF


