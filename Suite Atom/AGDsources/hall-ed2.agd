                    SUBTRACT 32 FROM Y
                    LET L 10
                ENDIF
            ENDIF
        ENDIF
    ENDIF
ENDIF
IF KEY DOWN
    IF DEADLY
        IF Y > 120
        ELSE
            IF E = 1
                IF L = 0
                    BEEP 20
                    ADD 32 TO Y
                    LET L 10
                ENDIF
            ENDIF
        ENDIF
    ENDIF
ENDIF
