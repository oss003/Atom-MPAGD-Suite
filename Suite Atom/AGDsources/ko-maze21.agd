EVENT MAINLOOP1

;--------------------------------------------------------------------
; Check viewchange flag in screen <> 0
;--------------------------------------------------------------------

  IF SCREEN > 0

    IF V > 0

; Draw default playfield

      COLOUR 4

      LET I 1
      REPEAT 16
        AT I,1: PRINT "                "
        ADD 1 TO I
      ENDREPEAT

;--------------------------------------------------------------------
; Every bit of variable Q draws outer wall
;--------------------------------------------------------------------

      IF Q > 127		; left wall straight	Q7
        AT  3 1: PRINT "uuu"
        LET I 4
        REPEAT 10
          AT  I 3: CHR 115
          ADD 1 TO I
        ENDREPEAT
        AT 14 1: PRINT "vvv"
      ELSE			; left wall cross
        AT  1 1: CHR 113
        AT  2 2: CHR 113
        AT  3 3: CHR 113
        LET I 4
        REPEAT 10
          AT  I 3: CHR 115
          ADD 1 TO I
        ENDREPEAT
        AT 14 3: CHR 112
        AT 15 2: CHR 112
        AT 16 1: CHR 112 
      ENDIF
      MULTIPLY Q BY 2

      IF Q > 127		; centre wall		Q6
        AT  3 4: PRINT "uuuuuuuuuu"
        AT 14 4: PRINT "vvvvvvvvvv"
      ELSE

;--------------------------------------------------------------------
; Draw view, every bit of variable P draws inner wall
;--------------------------------------------------------------------

      IF P > 127		; left wall straight	P7
        AT  6 4: PRINT "uuu"
        AT  7 6: CHR 115
        AT  8 6: CHR 115
        AT  9 6: CHR 115
        AT 10 6: CHR 115
        AT 11 4: PRINT "vvv"		
      ELSE			; left wall cross
        AT  4 4: CHR 113
        AT  5 5: CHR 113
        AT  6 6: CHR 113
        AT  7 6: CHR 115
        AT  8 6: CHR 115
        AT  9 6: CHR 115
        AT 10 6: CHR 115
        AT 11 6: CHR 112
        AT 12 5: CHR 112
        AT 13 4: CHR 112 
      ENDIF
      MULTIPLY P BY 2

      IF P > 127		; centre wall		P6
        AT  6  7: PRINT "uuuu"
        AT 11  7: PRINT "vvvv"
      ENDIF
      MULTIPLY P BY 2
 
      IF P > 127		; right wall straight	P5
        AT  6 11: PRINT "uuu"		
        AT  7 11: CHR 108
        AT  8 11: CHR 108
        AT  9 11: CHR 108
        AT 10 11: CHR 108
        AT 11 11: CHR 113
        AT 11 11: PRINT "vvv"		
      ELSE			; right wall cross
        AT  4 13: CHR 112
        AT  5 12: CHR 112
        AT  6 11: CHR 112
        AT  7 11: CHR 108
        AT  8 11: CHR 108
        AT  9 11: CHR 108
        AT 10 11: CHR 108
        AT 11 11: CHR 113
        AT 12 12: CHR 113
        AT 13 13: CHR 113 
      ENDIF
      ENDIF
      MULTIPLY Q BY 2
;--------------------------------------------------------------------

      IF Q > 127		; right wall straight	Q5
        AT  3 14: PRINT "uuu"
        LET I 4
        REPEAT 10
          AT  I 14: CHR 108
          ADD 1 TO I
        ENDREPEAT
        AT 14 14: PRINT "vvv"
      ELSE			; right wall cross
        AT 1 16: CHR 112
        AT 2 15: CHR 112
        AT 3 14: CHR 112
        LET I 4
        REPEAT 10
          AT  I 14: CHR 108
          ADD 1 TO I
        ENDREPEAT
        AT 14 14: CHR 113
        AT 15 15: CHR 113
        AT 16 16: CHR 113
      ENDIF


; Reset viewchange flag

      LET V 0

    ENDIF

  ENDIF

EVENT MAINLOOP2

;--------------------------------------------------------------------
; Test for introscreen
;--------------------------------------------------------------------

  IF SCREEN = 0
    INK 4
    AT 3 3: PRINT "SHIP:"
    AT 5 3: PRINT "CARGO:"
    AT 7 3: PRINT "CREW:"
    AT 9 3: PRINT "CAT NAME:"

; Display level Mission details

    IF L = 1
;      AT 1 1: DISPLAY L
      INK 7
      AT 3  9: PRINT "GALAXY"
      AT 5  9: PRINT "TEA BAGS"
      AT 7  9: PRINT "3"
      AT 9 13: PRINT "SEBASTIAN"
      WAITKEY
      LET SCREEN = 1
      RESTART
    ENDIF
  ELSE

;--------------------------------------------------------------------
; Adjust timer
;--------------------------------------------------------------------

  ADD 1 TO T
  IF T >= 20
    SUBTRACT 20 FROM T
    IF S > 0
      SUBTRACT 1 FROM S
      INK 2
      AT 22 23: DISPLAY SECONDS S
    ELSE
      KILL
    ENDIF
  ENDIF

;--------------------------------------------------------------------
; Display score and stuff
;--------------------------------------------------------------------

  INK 7
  AT 22 12: DISPLAY DOUBLEDIGITS LIVES
  AT 22  1: SHOWSCORE

  INK 2
  AT 21 21: PRINT "DESTRUCT"
  AT 22 24: PRINT ":"

 ENDIF


EVENT INTROMENU

  INK 4
  PAPER 0
  BORDER 0

  LET CONTROL = 99
  WHILE CONTROL >= 99
    CLS
    AT  8 10: PRINT "1. KEYBOARD"
    AT 10 10: PRINT "2. KEMPSTON"
    AT 12 10: PRINT "3. SINCLAIR"
    AT 14 10: PRINT "4. REDEFINE"
    LET CONTROL = 99
    WHILE CONTROL = 99
      IF KEY OPTION1
        LET CONTROL = 0
      ENDIF
      IF KEY OPTION2
        LET CONTROL = 1
      ENDIF
      IF KEY OPTION3
        LET CONTROL = 2
      ENDIF
      IF KEY OPTION4
        CLS
        AT 3 9: PRINT "PRESS KEY FOR:"
        WHILE KEY OPTION4
        ENDWHILE
        AT  6 12: PRINT "UP":
        DEFINEKEY UP
        AT  8 12: PRINT "DOWN"
        DEFINEKEY DOWN
        AT 10 12: PRINT "LEFT"
        DEFINEKEY LEFT
        AT 12 12: PRINT "RIGHT"
        DEFINEKEY RIGHT
        AT 14 12: PRINT "PAUSE"
        DEFINEKEY FIRE3
        LET CONTROL = 100
      ENDIF
    ENDWHILE
  ENDWHILE

  CLS


