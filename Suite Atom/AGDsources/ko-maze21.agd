EVENT PLAYER

;--------------------------------------------------------------------
; Test keys
;--------------------------------------------------------------------

  IF KEY RIGHT			; Rotate player clockwise
    ANIMATE
    WHILE KEY RIGHT
    ENDWHILE
    LET V 1			; Set view update flag
  ENDIF

  IF KEY LEFT			; Rotate player counter clockwise
    ANIMBACK
    WHILE KEY LEFT
    ENDWHILE
    LET V 1			; Set view update flag
  ENDIF

  IF KEY UP			; Move player forwards
    IF FRAME 0
      IF CANGOUP
        SUBTRACT 16 FROM Y	; Move up
        LET V 1			; Set view update flag
      ENDIF
    ENDIF
    IF FRAME 1
      IF CANGORIGHT
        ADD 16 TO X		; Move right
        LET V 1			; Set view update flag
      ENDIF
    ENDIF
    IF FRAME 2
      IF CANGODOWN
        ADD 16 TO Y		; Move down
        LET V 1			; Set view update flag
      ENDIF
    ENDIF
    IF FRAME 3
      IF CANGOLEFT
        SUBTRACT 16 FROM X	; Move left
        LET V 1			; Set view update flag
      ENDIF
    ENDIF
    WHILE KEY UP
    ENDWHILE
  ENDIF

;--------------------------------------------------------------------
; Test if view changed
;--------------------------------------------------------------------

  IF V 1

    LET P 0			; Reset bitmask innerview
    LET Q 0			; Reset bitmast outerview

;--------------------------------------------------------------------
; Create north view
;--------------------------------------------------------------------

    IF FRAME 0			; View north
      IF CANGOLEFT		; Create outerview
        ADD 128 TO Q		; Straight wall left
      ENDIF
      IF CANGORIGHT
        ADD 32 TO Q		; Straight wall right
      ENDIF
      IF CANGOUP
        SUBTRACT 16 FROM Y	; Create innerview
        IF CANGOLEFT
          ADD 128 TO P		; Straight wall left
        ENDIF
        IF CANGORIGHT
          ADD 32 TO P		; Straight wall right
        ENDIF
        IF CANGOUP
        ELSE
          ADD 64 TO P		; Center wall
        ENDIF
        ADD 16 TO Y
      ELSE
        ADD 64 TO Q		; Center wall
      ENDIF
    ENDIF

;--------------------------------------------------------------------
; Create east view
;--------------------------------------------------------------------

    IF FRAME 1			; View east
      IF CANGOUP		; Create outerview
        ADD 128 TO Q		; Straight wall left
      ENDIF
      IF CANGODOWN
        ADD 32 TO Q		; Straight wall right
      ENDIF
      IF CANGORIGHT
        ADD 16 TO X		; Create innerview
        IF CANGOUP
          ADD 128 TO P		; Straight wall left
        ENDIF
        IF CANGODOWN
          ADD 32 TO P		; Straight wall right
        ENDIF
        IF CANGORIGHT
        ELSE
          ADD 64 TO P		; Center wall
        ENDIF
        SUBTRACT 16 FROM X
      ELSE
        ADD 64 TO Q		; Center wall
      ENDIF
    ENDIF

;--------------------------------------------------------------------
; Create south view
;--------------------------------------------------------------------

    IF FRAME 2			; View south
      IF CANGORIGHT		; Create outerview
        ADD 128 TO Q		; Straight wall left
      ENDIF
      IF CANGOLEFT
        ADD 32 TO Q		; Straight wall right
      ENDIF
      IF CANGODOWN
        ADD 16 TO Y		; Create innerview
        IF CANGORIGHT
          ADD 128 TO P		; Straight wall left
        ENDIF
        IF CANGOLEFT
          ADD 32 TO P		; Straight wall right
        ENDIF
        IF CANGODOWN
        ELSE
          ADD 64 TO P		; Center wall
        ENDIF
        SUBTRACT 16 FROM Y
      ELSE
        ADD 64 TO Q		; Center wall
      ENDIF
    ENDIF

;--------------------------------------------------------------------
; Create west view
;--------------------------------------------------------------------

    IF FRAME 3			; View west
      IF CANGODOWN		; Create outerview
        ADD 128 TO Q		; Straight wall left
      ENDIF
      IF CANGOUP
        ADD 32 TO Q		; Straight wall right
      ENDIF
      IF CANGOLEFT
        SUBTRACT 16 FROM X	; Create innerview
        IF CANGODOWN
          ADD 128 TO P		; Straight wall left
        ENDIF
        IF CANGOUP
          ADD 32 TO P		; Cross wal right
        ENDIF
        IF CANGOLEFT
        ELSE
          ADD 64 TO P		; Center wall
        ENDIF
        ADD 16 TO X
      ELSE
        ADD 64 TO Q		; Center wall
      ENDIF
    ENDIF

  ENDIF


