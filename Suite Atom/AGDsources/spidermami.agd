        ENDIF
        IF COLLISION 0
            IF M > 0
                SUBTRACT 1 FROM M
            ENDIF
            ADD 1 TO P
            ADD 20 TO F
            REMOVE
            BEEP 57
        ENDIF
        IF CUSTOM
            IF M > 0
                SUBTRACT 1 FROM M
            ENDIF
            ADD 1 TO O
            LET SETTINGB 2
            LET IMAGE 11
            LET FRAME 0
            BEEP 22
        ENDIF
        IF DEADLY
            IF M > 0
                SUBTRACT 1 FROM M
            ENDIF
            ADD 1 TO O
            LET SETTINGB 2
            LET IMAGE 11
            LET FRAME 0
            BEEP 22
        ENDIF
        IF COLLISION 4
