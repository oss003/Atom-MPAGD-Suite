            ENDIF
            ADD 1 TO O
            LET SETTINGB 2
            LET IMAGE 11
            LET FRAME 0
            BEEP 22
        ENDIF
        EXIT
    ENDIF
    IF SETTINGB = 0
        GETRANDOM 4
        IF RND = 0
            SUBTRACT 1 FROM DIRECTION
            IF DIRECTION > 200
                LET DIRECTION 7
            ENDIF
        ELSE
            IF RND = 3
                ADD 1 TO DIRECTION
                IF DIRECTION > 7
                    LET DIRECTION 0
                ENDIF
            ENDIF
        ENDIF
        LET FRAME DIRECTION
        IF DIRECTION = 0
            REPEAT 2
                IF CANGOUP
                    SPRITEUP
                ENDIF
            ENDREPEAT
        ELSE
            IF DIRECTION = 1
                IF CANGOUP
                    SPRITEUP
                ENDIF
                IF CANGORIGHT
                    SPRITERIGHT
                ENDIF
            ELSE
                IF DIRECTION = 2
                    REPEAT 2
                        IF CANGORIGHT
                            SPRITERIGHT
                        ENDIF
                    ENDREPEAT
                ELSE
                    IF DIRECTION = 3
                        IF CANGORIGHT
                            SPRITERIGHT
                        ENDIF
                        IF CANGODOWN
                            SPRITEDOWN
                        ENDIF
                    ELSE
                        IF DIRECTION = 4
                            REPEAT 2
                                IF CANGODOWN
                                    SPRITEDOWN
                                ENDIF
                            ENDREPEAT
                        ELSE
                            IF DIRECTION = 5
                                IF CANGODOWN
                                    SPRITEDOWN
                                ENDIF
                                IF CANGOLEFT
                                    SPRITELEFT
                                ENDIF
                            ELSE
                                IF DIRECTION = 6
                                    REPEAT 2
                                        IF CANGOLEFT
                                            SPRITELEFT
                                        ENDIF
                                    ENDREPEAT
                                ELSE
                                    IF DIRECTION = 7
                                        IF CANGOLEFT
                                            SPRITELEFT
                                        ENDIF
                                        IF CANGOUP
                                            SPRITEUP
                                        ENDIF
                                    ENDIF
                                ENDIF
                            ENDIF
                        ENDIF
                    ENDIF
                ENDIF
            ENDIF
        ENDIF
        IF COLLISION 0
            ADD 1 TO P
            IF M > 0
                SUBTRACT 1 FROM M
            ENDIF
            BEEP 57
            REMOVE
            ; COLOUR 70
            LET COLUMN 9
            REPEAT P
                LET LINE 1
;                ADD 1 TO COLUMN
                MESSAGE 4
            ENDREPEAT
            ADD 10 TO G
            OTHER
            IF DIRECTION = 4
                ADD 10 TO I
            ENDIF
            ENDSPRITE
        ELSE
            IF COLLISION 1
                LET SETTINGB 1
                LET FRAME 8
                LET A 0
                OTHER
                REMOVE
                ENDSPRITE
            ELSE
                IF CUSTOM
                    ADD 1 TO O
                    IF M > 0
                        SUBTRACT 1 FROM M
                    ENDIF
                    BEEP 22
                    LET SETTINGB 2
                    LET IMAGE 11
                    LET FRAME 0
                ELSE
                    IF COLLISION 4
                        ADD 1 TO O
                        IF M > 0
                            SUBTRACT 1 FROM M
                        ENDIF
                        BEEP 22
                        LET SETTINGB 2
                        LET IMAGE 11
                        LET FRAME 0
                    ELSE
                        IF COLLISION 5
                            ADD 1 TO O
                            IF M > 0
                                SUBTRACT 1 FROM M
                            ENDIF
                            BEEP 22
                            LET SETTINGB 2
                            LET IMAGE 11
                            LET FRAME 0
                        ENDIF
                    ENDIF
                ENDIF
            ENDIF
        ENDIF
    ENDIF
