        IF CANGOUP
            SPRITEUP
        ELSE
            LET SETTINGB = 0
        ENDIF
    ELSE
        IF CANGODOWN
            SPRITEDOWN
        ELSE
            LET SETTINGB = 0
        ENDIF
    ENDIF
ENDIF


