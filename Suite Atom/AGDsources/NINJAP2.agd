EVENT PLAYER
IF A = 1
    ANIMATE
ENDIF
; COLOUR 3
;IF F <= 2
    ; BORDER 68
;ENDIF
;IF F > 3
    ; BORDER 0
;ENDIF
LET Z 0
IF CUSTOM
	LET Z 1
ENDIF
IF LADDERABOVE
	LET Z 1
ENDIF
IF LADDERBELOW
	LET Z 1
ENDIF
IF Z 1
    IF E = 0
    	  IF W < 50
    	     ADD 1 TO W
    	  ELSE
		     LET W 0    
			  SUBTRACT 1 FROM F
			  LET E = 90
		  ENDIF
    ELSE
        SUBTRACT 1 FROM E
    ENDIF
ENDIF
IF F = 0
    KILL
    BEEP 10
ENDIF
IF Y <= 17
    SCREENUP
    LET Y = 150
ENDIF
IF Y > 151
    SCREENDOWN
    LET Y = 18
ENDIF
IF X <= 8
    SCREENLEFT
    LET X = 230
    SUBTRACT 1 FROM Y
ENDIF
IF X > 230
    SCREENRIGHT
    LET X = 10
    SUBTRACT 1 FROM Y
ENDIF
DETECTOBJ
IF OBJ = 255
ELSE
    GET OBJ
    IF OBJ <= 31
        LET F = 12
        BEEP 5
    ELSE
        SUBTRACT 1 FROM H
    ENDIF
ENDIF
IF KEY RIGHT
    IF CANGORIGHT
        LET IMAGE = 3
        IF A = 0
            ANIMATE
            IF FRAME = 2
                BEEP 3
            ENDIF
        ENDIF
        SPRITERIGHT
    ENDIF
ENDIF
IF KEY LEFT
    IF CANGOLEFT
        LET IMAGE = 2
        IF A = 0
            ANIMATE
            IF FRAME = 2
                BEEP 3
            ENDIF
        ENDIF
        SPRITELEFT
    ENDIF
ENDIF
IF KEY FIRE
    TABLEJUMP
ENDIF
IF CANGODOWN
    IF COLLISION 0
    ELSE
        TABLEFALL
    ENDIF
ENDIF
IF DEADLY
    KILL
    BEEP 10
ENDIF
IF KEY UP
    IF LADDERABOVE
        LET FRAME = 0
        LET IMAGE = 4
        IF A = 0
            ANIMATE
            IF FRAME = 1
                BEEP 3
            ENDIF
        ENDIF
        SPRITEUP
    ENDIF
ENDIF
IF KEY DOWN
    IF LADDERBELOW
        LET FRAME = 0
        LET IMAGE = 4
        IF A = 0
            ANIMATE
            IF FRAME = 1
                BEEP 3
            ENDIF
        ENDIF
        SPRITEDOWN
    ENDIF
ENDIF


