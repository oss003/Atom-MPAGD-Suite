EVENT SPRITETYPE1
 SPRITEINK 7
IF Y = 200
    GETRANDOM 60
    IF RND = 1
        GETRANDOM 52
        MULTIPLY RND BY 2
        LET X RND
        ADD 60 TO X
        GETRANDOM 52
        MULTIPLY RND BY 2
        LET Y RND
        ADD 24 TO Y
        LET IMAGE 5
        LET SETTINGB = 40
        EXIT
    ENDIF
ELSE
    IF IMAGE = 5
        LET FRAME 0
        GETRANDOM 6
        ADD 1 TO RND
        ; SPRITEINK RND
        SUBTRACT 1 TO SETTINGB
        IF SETTINGB = 0
            GETRANDOM 3
            ADD 6 TO RND
            LET IMAGE RND
            LET FRAME 0
            EXIT
        ENDIF
    ELSE
        IF DIRECTION = 1
            ADD 254 TO Y
        ELSE
            IF 5 > DIRECTION
                ADD 2 TO X
            ELSE
                IF DIRECTION > 5
                    ADD 254 TO X
                ENDIF
            ENDIF
        ENDIF
        IF DIRECTION = 2
            ADD 254 TO Y
        ENDIF
        IF DIRECTION = 8
            ADD 254 TO Y
        ENDIF
        IF DIRECTION > 3
            IF 7 > DIRECTION
                ADD 2 TO Y
            ENDIF
        ENDIF
        LET Q = 0
        IF SETTINGB = 0
            IF CUSTOM
                LET Q = 1
                IF DIRECTION = 8
                    LET DIRECTION = 0
                ENDIF
            ELSE
                IF DEADLY
                    LET Q = 255
                    IF DIRECTION = 1
                        LET DIRECTION = 9
                    ENDIF
                ENDIF
            ENDIF
            IF Q <> 0
                LET SETTINGB = 4
            ENDIF
            ADD Q TO DIRECTION
            LET Q = 0
        ENDIF
        IF SETTINGB > 0
            SUBTRACT 1 FROM SETTINGB
        ENDIF
        LET FRAME = 0
        IF COLLISION 10
            SUBTRACT 1 FROM SETTINGA
            IF SETTINGA = 0
                EXPLODE 26
                LET SETTINGA = 1
                LET DIRECTION = 7
                LET X = S
                ADD 16 TO S
                IF S > 224
                    LET S = 176
                ENDIF
                GETRANDOM 52
                MULTIPLY RND BY 2
                LET Y RND
                IF COLLISION 1
                    SUBTRACT 16 FROM X
                ENDIF
            ENDIF
            IF 8 > X
                LET X = 220
            ENDIF
            IF X > 220
                LET X = 8
            ENDIF
            IF 8 > Y
                LET Y = 170
            ENDIF
            IF Y > 170
                LET Y = 8
            ENDIF
        ENDIF
        IF IMAGE > 5
            SUBTRACT 3 TO IMAGE
            ; SPRITEINK IMAGE
            ADD 3 TO IMAGE
        ENDIF
    ENDIF
ENDIF


