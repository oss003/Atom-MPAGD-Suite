        LET IMAGE 2
        IF A = 0
            ANIMATE
        ENDIF
        SPRITEUP
    ENDIF
ENDIF
IF KEY DOWN
    IF LADDERBELOW
        LET IMAGE 2
        IF A = 0
            ANIMATE
        ENDIF
        SPRITEDOWN
    ENDIF
ENDIF
IF KEY RIGHT
    IF CANGORIGHT
        IF KEY UP
            IF LADDERABOVE
            ELSE
                LET IMAGE 1
                IF A = 0
                    ANIMATE
                ENDIF
                SPRITERIGHT
            ENDIF
        ELSE
            LET IMAGE 1
            IF A = 0
                ANIMATE
            ENDIF
            SPRITERIGHT
        ENDIF
    ENDIF
ENDIF
