    ; COLOUR 71
    DISPLAY H
    IF G <= 99
        LET LINE 4
        IF G <= 9
            LET COLUMN 27
        ELSE
            LET COLUMN 26
        ENDIF
        ; COLOUR 66
        DISPLAY G
    ENDIF
    IF COLLISION 1
        OTHER
        IF IMAGE = 1
            ; SOUND 1
            IF H > 0
                SUBTRACT 1 FROM H
            ENDIF
        ENDIF
        ENDSPRITE
        IF D > 1
            SUBTRACT 1 FROM D
        ENDIF
    ENDIF
    LET E 20
    ADD 6 TO D
    LET LINE 17
    LET COLUMN D
    ; COLOUR 68
    MESSAGE 2
    IF D <= 21
