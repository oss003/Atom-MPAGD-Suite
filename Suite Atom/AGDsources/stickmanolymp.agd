        LET LINE 17
        LET COLUMN D
        ; COLOUR 65
        MESSAGE 2
        SUBTRACT 2 FROM D
    ENDIF
    SUBTRACT 6 FROM D
    IF FRAME = 2
        LET C = 1
    ELSE
        LET C = 0
    ENDIF
    IF KEY RIGHT
        IF D > 1
            IF K = 1
                SUBTRACT 1 FROM D
            ENDIF
        ENDIF
        IF B = 0
            ANIMATE
            IF D <= 15
                ADD 1 TO D
            ENDIF
        ENDIF
        IF D > 0
            ANIMATE
        ENDIF
        LET B = 1
    ELSE
        IF A = 0
            IF D > 0
                SUBTRACT 1 FROM D
                ANIMATE
            ENDIF
        ENDIF
        LET B = 0
        IF FRAME <> 0
            ANIMATE
        ENDIF
    ENDIF
    IF KEY FIRE
        TABLEJUMP
    ENDIF
