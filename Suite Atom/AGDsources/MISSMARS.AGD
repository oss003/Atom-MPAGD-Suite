EVENT PLAYER
TABLEFALL
DETECTOBJ
IF OBJ = 255
ELSE
    IF OBJ = 0
        GET OBJ
        LET LINE = 2
        LET COLUMN = 4
        ; COLOUR 4
        MESSAGE 2
        DELAY 40
        WAITKEY
        REDRAW
	ELSE
		 GET OBJ
		 SCORE 100
		 LET LINE = 23
		 LET COLUMN = 20
		 SHOWSCORE
		 BEEP 10
   ENDIF
ENDIF
; warning - ENDIF without IF?
;ENDIF
IF KEY UP
    IF LADDERABOVE
        SPRITEUP
        IF FRAME >= 2
	        BEEP 40
		  ENDIF
        ;LET IMAGE = 6
        IF A = 0
            ANIMATE
        ENDIF
    ENDIF
ENDIF
IF KEY DOWN
    IF LADDERBELOW
        SPRITEDOWN
        IF FRAME >= 2
	        BEEP 45
		  ENDIF
        ;LET IMAGE = 6
        IF A = 0
            ANIMATE
        ENDIF
    ENDIF
ENDIF
IF KEY RIGHT
    IF X > = 230
    	  SCORE 75
		  LET LINE = 23
		  LET COLUMN = 20
		  SHOWSCORE
        SCREENRIGHT
        LET X = 10
        SUBTRACT 1 FROM Y
    ENDIF
    IF CANGORIGHT
        LET IMAGE = 1
        ANIMATE
        SPRITERIGHT
        IF FRAME = 2
	        BEEP 10
		  ENDIF
    ENDIF
ENDIF
; warning - ENDIF without IF?
;ENDIF
IF KEY LEFT
    IF X <= 8
        ;SCREENLEFT
        ;LET X = 229
        ;EXIT
    ELSE
        IF CANGOLEFT
            LET IMAGE = 0
            ANIMATE
            SPRITELEFT
			  IF FRAME = 1
				  BEEP 10
			  ENDIF
        ENDIF
    ENDIF
ENDIF
IF KEY UP
    TABLEJUMP
ENDIF
IF DEADLY
    KILL
ENDIF


